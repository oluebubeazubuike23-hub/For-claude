local function AddBattleRoyaleTextToParent(p64, p65)
	local v157 = Instance.new("TextLabel") 
	v157.TextScaled = true 
	v157.Text = p64.Name 
	v157.Size = UDim2.new(1, 0, 0, 25) 
	v157.Name = p64.Name 
