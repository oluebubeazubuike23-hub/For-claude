-- skeleton slash (one of the stomp effect modulescript)

local v1 = {};
local l__ReplicatedStorage__2 = game.ReplicatedStorage;
local l__TweenService__6 = game:GetService("TweenService");
local l__Debris__7 = game:GetService("Debris");

function v1.RunStompFx(p1, p2, p3, p4, p5)
	-- p1 = StompFX name
	-- p2 = victim's HumanoidRootPart
	-- p3 = victim's character/humanoid parent
	-- p4 = LocalPlayer (attacker)
	-- p5 = callback table (unused)

	if workspace:FindFirstChild(p3.Name .. "-Skeleton Slash") == nil then
		local LocalPlayer = p4;
		local VFX_upvr = script.VFX;

		-- Clone and setup skeleton model
		local clone_upvr = VFX_upvr.Skeleton:Clone();
		clone_upvr.Name = p3.Name .. "-Skeleton Slash";
		l__Debris__7:AddItem(clone_upvr, 7);

		-- Position skeleton
		local var46;
		if p2.Parent and p2.Parent.PrimaryPart then
			var46 = CFrame.new(p2.Parent.PrimaryPart.Position);
		else
			return;
		end;

		-- Setup initial sound
		local clone_2 = VFX_upvr.SFX2:Clone();
		clone_2.Parent = clone_upvr;
		clone_2:Play();

		-- Position and parent skeleton
		clone_upvr:SetPrimaryPartCFrame(var46 * CFrame.Angles(p2.Parent.PrimaryPart.CFrame:ToOrientation()) * CFrame.new(21, 82.5, 59));
		clone_upvr.Parent = workspace.Ignored.Animations;

		-- Distance check and lighting effect
		local var52_upvw;
		if LocalPlayer.Character and LocalPlayer.Character.PrimaryPart then
			var52_upvw = (clone_upvr.PrimaryPart.Position - LocalPlayer.Character.PrimaryPart.Position).Magnitude;
			if var52_upvw <= 500 then
				local clone = VFX_upvr.StompFX:Clone();
				clone.Parent = game:GetService("Lighting");
				l__TweenService__6:Create(clone, TweenInfo.new(3), {
					TintColor = Color3.fromRGB(47, 47, 47);
				}):Play();
				l__Debris__7:AddItem(clone, 5);
			end;
		end;

		-- Add highlight
		VFX_upvr.Highlight:Clone().Parent = clone_upvr;

		-- Make parts non-collidable
		for _, v in pairs(clone_upvr:GetDescendants()) do
			if v:IsA("BasePart") then
				v.CanCollide = false;
			end;
		end;

		-- Play animation
		clone_upvr.Humanoid:LoadAnimation(script.Animation):Play();

		-- Main slash effects after delay
		local CameraShaker_upvr = require(script.Parent.Parent.Modules.CameraShaker);
		local CurrentCamera_upvr = workspace.CurrentCamera;
		local clone_6_upvr = VFX_upvr.SFX1:Clone();

		task.delay(1.5, function()
			-- Camera shake
			if var52_upvw and var52_upvw <= 500 then
				local camShake = CameraShaker_upvr.new(Enum.RenderPriority.Camera.Value, function(shakeCf)
					CurrentCamera_upvr.CFrame *= shakeCf;
				end);
				camShake:Start();
				camShake:ShakeOnce(25, 5, 1.5, 0.5);
			end;

			-- Play slash sound
			clone_6_upvr.Parent = clone_upvr;
			clone_6_upvr.PlayOnRemove = true;
			clone_6_upvr:Play();
			clone_6_upvr:Destroy();

			-- Spawn slash beams (Heaven Judgment style)
			for i = 1, 3 do
				task.wait(0.3);

				-- Create dark slash beam
				local v13 = Instance.new("Part");
				v13.Parent = workspace.Ignored.Animations;
				v13.Anchored = true;
				v13.CanCollide = false;
				v13.Color = Color3.fromRGB(47, 47, 47);
				v13.Transparency = 0.3;
				v13.Material = Enum.Material.Neon;
				v13.Shape = Enum.PartType.Block;
				l__Debris__7:AddItem(v13, 3);

				-- Calculate slash position from sky
				local basePos = p2.Position + Vector3.new(math.random(-15, 15), 0, math.random(-15, 15));
				local v16 = Vector3.new(basePos.X, basePos.Y + 200, basePos.Z);
				local v17 = Vector3.new(basePos.X, basePos.Y - 20, basePos.Z);

				local v18, v19 = workspace:FindPartOnRayWithIgnoreList(
					Ray.new(v16, (v17 - v16).unit * 300),
					{ workspace.Ignored, workspace.Characters }
				);

				local l__Magnitude__20 = (v16 - v19).Magnitude;
				v13.Size = Vector3.new(3, l__Magnitude__20, 10);
				v13.CFrame = CFrame.new(v16, v17) * CFrame.new(0, -l__Magnitude__20 / 2, 0);

				-- Animate slash expansion and fade
				coroutine.wrap(function()
					local v23 = l__TweenService__6:Create(v13, TweenInfo.new(0.3), {
						Transparency = 0.5,
						Size = Vector3.new(10, l__Magnitude__20, 25)
					});
					v23:Play();
					v23.Completed:Wait();

					l__TweenService__6:Create(v13, TweenInfo.new(1), {
						Transparency = 1,
						Size = Vector3.new(0, l__Magnitude__20, 0)
					}):Play();
				end)();
			end;

			-- Emit particles
			for _, v_2 in pairs(VFX_upvr.Particles:GetChildren()) do
				local particleClone = v_2:Clone();
				particleClone.Parent = p2;
				task.wait(0.5);
				particleClone:Emit(500);
				l__Debris__7:AddItem(particleClone, 2);
			end;
		end);

		-- Fade out skeleton after 4 seconds
		task.delay(4, function()
			for i_3, v_3 in pairs(clone_upvr:GetDescendants()) do
				if v_3:IsA("BasePart") then
					l__TweenService__6:Create(v_3, TweenInfo.new(1), {
						Transparency = 1;
					}):Play();
				elseif v_3:IsA("Highlight") then
					l__TweenService__6:Create(v_3, TweenInfo.new(1), {
						OutlineTransparency = 1;
					}):Play();
				elseif v_3:IsA("ParticleEmitter") then
					v_3.Enabled = false;
				end;
			end;

			-- Fade lighting effect back to normal
			local lightingEffect = game:GetService("Lighting"):FindFirstChild("StompFX");
			if lightingEffect then
				l__TweenService__6:Create(lightingEffect, TweenInfo.new(1), {
					Saturation = 0.35,
					TintColor = Color3.fromRGB(255, 255, 255)
				}):Play();
			end;

			l__Debris__7:AddItem(clone_upvr, 2);
		end);
	end;
	return nil;
end;

return v1;


--stomp handler (from game 2)
local TweenService = game:GetService('TweenService')
local RunService = game:GetService('RunService')
MainEvent = game.ReplicatedStorage.MainEvent

local MainModule = require(game.ReplicatedStorage.MainModule)

local function ServerStomp(Player, Argument1,Argument2, Argument3)
	print(Player, Argument1, Argument2, Argument3)
	if _G.PlayerData.Players[Player].Debounces.Action.Stomp ~= false then
		print("ended")
		return
	end
	pcall(function()
		task.spawn(function()
			_G.PlayerData.Players[Player].Debounces.Action.Stomp = true
			task.wait(1)
			_G.PlayerData.Players[Player].Debounces.Action.Stomp = false
		end)
	end)
	local Part = Argument3
	print("part " .. Part.Parent.Name)
	if game.Players[Player.Name].Character then
		print(Player)
		if not MainModule.Activate(Argument2) then
			print("ended2")
			return
		end
		print(Part)
		task.spawn(function()
			if Part.Name == 'HumanoidRootPart' then
				if Part.Parent:FindFirstChild('UpperTorso') then
					Part = Part.Parent.UpperTorso
				end
			end
			local Particle = Instance.new('ParticleEmitter', Part)
			Particle.Name = 'BloodParticle'
			Particle.LightEmission = 0
			Particle.Size = NumberSequence.new(0.3, 2)
			Particle.Texture = 'rbxassetid://2271208303'
			Particle.Transparency = NumberSequence.new(0.5, 1)
			Particle.ZOffset = 0.5
			Particle.Lifetime = NumberRange.new(0.5)
			Particle.Rate = 100
			Particle.Rotation = NumberRange.new(-360, 360)
			Particle.Speed = NumberRange.new(9)
			Particle.Enabled = false
			Particle.Acceleration = Vector3.new(Random.new():NextNumber(-25, 25), Random.new():NextNumber(-100, -50), Random.new():NextNumber(-25, 25))
			Particle:Emit(5)
			game:GetService('Debris'):AddItem(Particle, 1)
		end)
		task.spawn(function()
			local Humanoid
			if Part.Parent:FindFirstChildOfClass('Humanoid') then
				Humanoid = Part.Parent:FindFirstChildOfClass('Humanoid')
			elseif Part.Parent.Parent:FindFirstChildOfClass('Humanoid') then
				Humanoid = Part.Parent.Parent:FindFirstChildOfClass('Humanoid')
			elseif Part.Parent.Parent.Parent:FindFirstChildOfClass('Humanoid') then
				Humanoid = Part.Parent.Parent.Parent:FindFirstChildOfClass('Humanoid')
			end
			print(Humanoid.Parent.Name)
			if Humanoid and Humanoid.Parent.BodyEffects['K.O'].Value ~= false then
				if Humanoid.Parent.BodyEffects['Dead'].Value == false then
					if Humanoid.Parent:FindFirstChild('GRABBING_CONSTRAINT') then
						print("ended3")
						return
					end
					if Player.DataFolder.Officer.Value == 1 then
						Humanoid.Parent.BodyEffects['Dead'].Value = true
						Player.DataFolder.Officer.Value = 0
						Player.DataFolder.Information.Wanted.Value = 0
						Player.leaderstats.Wanted.Value = Player.DataFolder.Information.Wanted.Value
						Player:LoadCharacter()
						print("ended4")
						return
					end
					Player.DataFolder.Information.Wanted.Value += 50
					Player.leaderstats.Wanted.Value = Player.DataFolder.Information.Wanted.Value
					Humanoid.Parent.BodyEffects['Dead'].Value = true
					pcall(function()
						local Plr = game.Players:GetPlayerFromCharacter(Humanoid.Parent)
						if Plr.DataFolder.Officer.Value == 0 then
							if Plr.DataFolder.Information.Wanted.Value >= 40 then
								Plr.DataFolder.Information.Wanted.Value -= 40 
								Plr.leaderstats.Wanted.Value = Plr.DataFolder.Information.Wanted.Value
							else
								Plr.DataFolder.Information.Wanted.Value = 0
								Plr.leaderstats.Wanted.Value = Plr.DataFolder.Information.Wanted.Value
							end
						else 
							local Money = math.floor(math.abs(Plr.DataFolder.Information.Wanted.Value/2))
							Player.DataFolder.Currency.Value += Money 
							Plr.DataFolder.Information.Wanted.Value = 0
							Plr.leaderstats.Wanted.Value = Plr.DataFolder.Information.Wanted.Value
						end
						Plr.DataFolder.Information.ArmorSave.Value = 0
					end)

					task.spawn(function()
						local Sound = Instance.new('Sound', Part)
						Sound.Name = 'BloodSplatter'
						Sound.Volume = 0.5
						Sound.SoundId = 'rbxassetid://330595293'
						Sound:Play()
						Sound.Ended:Connect(function()
							Sound:Destroy()
						end)
					end)
				end
			end
		end)
	end
end

local function RegularStomp(Player, Argument1,Argument2, BodyEffects)
	local v29 = workspace:FindPartOnRayWithIgnoreList(Ray.new(Player.Character.LowerTorso.Position, Vector3.new(0, -Player.Character.UpperTorso.Size.y * 4.5, 0)), { Player.Character, unpack(MainModule.Ignored) }) 
	local v30 = nil 
	if v29 and v29:IsDescendantOf(workspace.Players) then
		v30 = v29.Parent:FindFirstChild("Humanoid") or (v29.Parent.Parent:FindFirstChild("Humanoid") or v29.Parent.Parent.Parent:FindFirstChild("Humanoid")) 
	end 
	if v30 then
		ServerStomp(Player, Argument1, BodyEffects, v29)
		local StompFX = Player.DataFolder.Information.StompFX.Value
		if StompFX == 'Dimension Slash' then
			local KillPlayer = Instance.new('ObjectValue', Player.Character)
			KillPlayer.Name = 'KillPlayer'
			KillPlayer.Value = Player.Character.HumanoidRootPart
			game.Debris:AddItem(KillPlayer, 8)
		end
		MainEvent:FireAllClients('FX_KILL', StompFX, v30.Parent.HumanoidRootPart, Player)
	end 
	return
end

MainEvent.OnServerEvent:Connect(function(Player, Subject, Argument1, Argument2, Argument3)
	local Stomp = Player.Character.Humanoid:LoadAnimation(game:GetService("ReplicatedStorage").ClientAnimations.Stomp) 
	if Subject == "Stomp" then
		RegularStomp(Player, Argument1,Stomp, Argument3)
	end
end)

