			v289.Size = Vector3.new(0, 0, 0) 
			v289.CFrame = v264.HumanoidRootPart.CFrame 
			v266:Stop() 
			v267:Play() 
			task.wait(0.5) 
			l__TweenService__207:Create(v289, TweenInfo.new(0.5, Enum.EasingStyle.Linear), {
				CFrame = v289.CFrame + Vector3.new(0, 4, 0), 
				Size = l__Size__292
			}):Play() 
