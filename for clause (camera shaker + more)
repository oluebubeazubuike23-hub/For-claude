				task.wait(0.6) 
			end 
			if v269.PlaybackState == Enum.PlaybackState.Playing then
				v269:Cancel() 
			end 
			l__TweenService__207:Create(v268, TweenInfo.new(0.4, Enum.EasingStyle.Linear), {
				Volume = 0
			}):Play() 
			l__TweenService__207:Create(l__CrateOpening__211.Entries, TweenInfo.new(0.4, Enum.EasingStyle.Linear), {
				Size = UDim2.new(1, 0, 0, 0)
			}):Play() 
			l__TweenService__207:Create(l__CrateOpening__211.Crate, TweenInfo.new(0.4, Enum.EasingStyle.Linear), {
				Size = UDim2.new(1, 0, 0.822, 0)
			}):Play() 
			task.wait(0.4) 
			v268:Destroy() 
			l__CrateOpening__211.Entries.Visible = false 
			l__TweenService__207:Create(v265, TweenInfo.new(0.75, Enum.EasingStyle.Bounce), {
				FieldOfView = 70, 
				CFrame = CFrame.new(v264.HumanoidRootPart.Position + v264.HumanoidRootPart.CFrame.LookVector * 8 + v264.HumanoidRootPart.CFrame.UpVector * 8, v264.HumanoidRootPart.Position)
			}):Play() 
			local v288
			if p69 == "[Knife]" then
				v288 = l__Knives__206.Default:Clone() 
			else
				v288 = l__GunModels__205[p69]:Clone() 
			end 
