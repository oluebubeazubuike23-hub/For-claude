local u68 = false 
local misctable = {} 
local function updateLeaderboard()
	if u68 == false then
		u68 = true 
		for v301, v302 in pairs(misctable) do
			pcall(function()
				v302:disconnect() 
			end) 
		end 
		for v303, v304 in pairs(MainScreenGui.Leaderboard.PlayerScroll:GetChildren()) do
			v304:Destroy() 
		end 
		for v305, v306 in pairs(MainScreenGui.Leaderboard.OfficerScroll:GetChildren()) do
			v306:Destroy() 
		end 
		local v307 = {} 
		local v308 = {} 
		local v309 = {} 
		local v310 = {} 
		for v311, v312 in pairs((game.Players:GetChildren())) do
			pcall(function()
				if v312 ~= LocalPlayer then
					if v312.DataFolder.Officer.Value == 0 then
						table.insert(v307, v312) 
					else
						table.insert(v308, v312) 
					end 
				end 
				table.insert(misctable, (v312.DataFolder.Officer.Changed:connect(function()
					updateLeaderboard() 
				end))) 
			end) 
		end 
		pcall(function()
			table.sort(v307, function(p77, p78)
				return p78.leaderstats.Wanted.Value < p77.leaderstats.Wanted.Value 
			end) 
			table.sort(v308, function(p79, p80)
				return p79.leaderstats.Wanted.Value < p80.leaderstats.Wanted.Value 
			end) 
		end) 
		if LocalPlayer.DataFolder.Officer.Value == 0 then
			table.insert(v309, LocalPlayer) 
		else
			table.insert(v310, LocalPlayer) 
		end 
		for v313 = 1, #v307 do
			table.insert(v309, v307[v313]) 
		end 
		for v314 = 1, #v308 do
			table.insert(v310, v308[v314]) 
		end 
		local u71 = 0 
		for v315, v316 in pairs(v309) do
			pcall(function()
				local v317 = MainModule.GroupImage(v316) 
				local v318 = Instance.new("TextLabel") 
				v318.Text = v316.Name 
				v318.BackgroundTransparency = 1 
				v318.Size = UDim2.new(0.35, 0, 0, 25) 
				v318.Position = UDim2.new(0, 25, 0, 30 * u71 + 5) 
				v318.TextColor3 = Color3.new(1, 1, 1) 
				v318.TextSize = 11 
				v318.TextWrapped = false 
				v318.TextXAlignment = Enum.TextXAlignment.Right 
				v318.Font = Enum.Font.GothamBlack 
				v318.Parent = MainScreenGui.Leaderboard.PlayerScroll 
				local v319 = v318:Clone() 
				v319.Size = UDim2.new(0, 25, 0, 25) 
				v319.Position = UDim2.new(0.65, 0, 0, 30 * u71 + 5) 
				v319.Text = MainModule.AddComma(v316.leaderstats.Wanted.Value) 
				v319.TextXAlignment = Enum.TextXAlignment.Center 
				v319.Parent = MainScreenGui.Leaderboard.PlayerScroll 
				if v317 then
					local v320 = Instance.new("ImageLabel") 
					v320.Size = UDim2.new(0, 25, 0, 25) 
					v320.BackgroundTransparency = 1 
					v320.Position = UDim2.new(0, 2, 0, v318.Position.Y.Offset) 
					v320.Image = v317 
					v320.Parent = MainScreenGui.Leaderboard.PlayerScroll 
				end 
				table.insert(misctable, (v316.leaderstats.Wanted.Changed:connect(function()
					v319.Text = MainModule.AddComma(v316.leaderstats.Wanted.Value) 
				end))) 
				u71 = u71 + 1 
			end) 
		end 
		MainScreenGui.Leaderboard.PlayerScroll.CanvasSize = UDim2.new(0, 0, 0, (30 * u71) + 5) 
		local u72 = 0 
		for v321, v322 in pairs(v310) do
			pcall(function()
				local v323 = MainModule.GroupImage(v322) 
				local v324 = Instance.new("TextLabel") 
				v324.Text = v322.Name 
				v324.BackgroundTransparency = 1 
				v324.Size = UDim2.new(0.35, 0, 0, 25) 
				v324.Position = UDim2.new(0, 25, 0, 30 * u72 + 5) 
				v324.TextColor3 = Color3.new(1, 1, 1) 
				v324.TextSize = 11 
				v324.TextWrapped = false 
				v324.TextXAlignment = Enum.TextXAlignment.Right 
				v324.Font = Enum.Font.GothamBlack 
				v324.Parent = MainScreenGui.Leaderboard.OfficerScroll 
				local v325 = v324:Clone() 
				v325.Size = UDim2.new(0, 25, 0, 25) 
				v325.Position = UDim2.new(0.65, 0, 0, 30 * u72 + 5) 
				v325.Text = MainModule.AddComma(math.abs(v322.leaderstats.Wanted.Value)) 
				v325.TextXAlignment = Enum.TextXAlignment.Center 
				v325.Parent = MainScreenGui.Leaderboard.OfficerScroll 
				if v323 then
					local v326 = Instance.new("ImageLabel") 
					v326.Size = UDim2.new(0, 25, 0, 25) 
					v326.BackgroundTransparency = 1 
					v326.Position = UDim2.new(0, 2, 0, v324.Position.Y.Offset) 
					v326.Image = v323 
					v326.Parent = MainScreenGui.Leaderboard.OfficerScroll 
				end 
				table.insert(misctable, (v322.leaderstats.Wanted.Changed:connect(function()
					v325.Text = MainModule.AddComma(math.abs(v322.leaderstats.Wanted.Value)) 
				end))) 
				u72 = u72 + 1 
			end) 
		end 
		MainScreenGui.Leaderboard.OfficerScroll.CanvasSize = UDim2.new(0, 0, 0, (30 * u72) + 5) 
		u68 = false 
	end 
end 
