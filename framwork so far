local Tool = {
	timer = tick(), 
	times = 0, 
	disabling = false
};
LocalPlayer.Backpack.ChildAdded:connect(function(p52)
	if p52:IsA("Tool") then
		if tick() - Tool.timer < 0.2 then
			Tool.times = Tool.times + 1;
		else
			Tool.times = 0;
		end;
		if Tool.times >= 4 and Tool.disabling == false then
			Tool.disabling = true;
			StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false);
			delay(1, function()
				Tool.disabling = false;
				StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true);
			end);
		end;
		Tool.timer = tick();
	end;
end);
BodyEffects.ChildAdded:connect(function(p53)
	if p53.Name == "Block" then
		Blocking(true);
	end;
end);
BodyEffects.ChildRemoved:connect(function(p54)
	if p54.Name == "Block" then
		Blocking(false);
	end;
end);

