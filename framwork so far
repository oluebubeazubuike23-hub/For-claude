	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Headless.SwitchFrame.Switch.BackgroundColor3 = Color3.new(0, 1, 0);
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Headless.SwitchFrame.Switch.Position = UDim2.new(0.5, 0, 0, 0);
