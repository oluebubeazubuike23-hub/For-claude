local function PopulateStompFrame()
	print("[DEBUG] PopulateStompFrame called!")

	for _, v in ipairs(StompFrame:GetChildren()) do
		if v:IsA("TextButton") and v ~= Template then
			print("[DEBUG] Destroying old button:", v.Name)
			v:Destroy()
		end
	end

	local Players = game:GetService("Players")
	local MarketplaceService = game:GetService("MarketplaceService")

	local Player = Players.LocalPlayer
	local StompModule = require(game.ReplicatedStorage.StompModule)

	local Players = game:GetService("Players")
	local Player = Players.LocalPlayer
	local PlayerGui = Player:WaitForChild("PlayerGui")

	local MainGUI = PlayerGui:WaitForChild("MainScreenGui") -- your ScreenGui name
	local StompFrame = MainGUI:WaitForChild("StompFrame")
	local Template = StompFrame:WaitForChild("StompButton")

	--------------------------------------------------
	-- üîì FREE ACCESS CONTROLS (EDIT HERE)
	--------------------------------------------------

	local FreeStomps = {
		-- ["Regular"] = true,
	}

	local FreeUsers = {
		-- [Player.UserId] = true,
	}

	--------------------------------------------------

	local function playerOwnsStomp(stompName, data)
		if FreeUsers[Player.UserId] then
			return true
		end

		if FreeStomps[stompName] then
			return true
		end

		if not data.GamepassId then
			return false
		end

		local success, owns = pcall(function()
			return MarketplaceService:UserOwnsGamePassAsync(Player.UserId, data.GamepassId)
		end)

		return success and owns
	end

	--------------------------------------------------

	for stompName, data in pairs(StompModule) do
		local button = Template:Clone()
		button.Visible = true
		button.Name = stompName
		button.Parent = StompFrame

		-- üè∑ Name
		button.SkinInfo.Container.NameLabel1.Text = stompName

		-- üñº Icon
		local iconFrame = button.Preview:WaitForChild("Icon")

		-- If Icon contains an ImageLabel inside
		local imageLabel = iconFrame:FindFirstChildWhichIsA("ImageLabel", true)
		if imageLabel and data.Icon then
			imageLabel.Image = data.Icon
		else
			-- Fallback styling if it's just a Frame
			iconFrame.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
		end


		-- üîê Ownership check
		local owns = playerOwnsStomp(stompName, data)

		--------------------------------------------------
		-- BUY BUTTON (OPTIONAL)
		--------------------------------------------------
		local buyButton = button:FindFirstChild("BuyButton", true)

		if buyButton then
			buyButton.Visible = not owns
			buyButton.MouseButton1Click:Connect(function()
				if data.GamepassId then
					print("[STOMP] Prompting purchase:", stompName)
					MarketplaceService:PromptGamePassPurchase(Player, data.GamepassId)
				end
			end)
		end

		--------------------------------------------------
		-- CLICK TO EQUIP (FRAME CLICK)
		--------------------------------------------------
		button.InputBegan:Connect(function(input)
			if input.UserInputType ~= Enum.UserInputType.MouseButton1 then
				return
			end

			if not owns then
				warn("[STOMP] Not owned:", stompName)
				return
			end

			print("[STOMP] Equipped:", stompName)
			-- üî• FIRE YOUR EQUIP / REMOTE EVENT HERE
		end)
	end

	for _, stompModule in ipairs(StompModules:GetChildren()) do
		if stompModule:IsA("ModuleScript") then
			print("[DEBUG] Creating button for:", stompModule.Name)
			local button = Template:Clone()
			button.Name = stompModule.Name
			button.Text = stompModule.Name
			button.Visible = true
			button.Parent = StompFrame

			button.MouseButton1Click:Connect(function()
				print("[CLIENT] Selecting stomp:", stompModule.Name)
				MainRemoteEvent:FireServer("SetStompFX", stompModule.Name)
			end)
		end
	end

	print("[DEBUG] PopulateStompFrame finished!")
end



