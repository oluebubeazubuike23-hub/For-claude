local u11 = false;
local function v27()
	if u11 == false and MainModule.Activate(BodyEffects) then
		u11 = true;
		local v28 = workspace:FindPartOnRayWithIgnoreList(Ray.new(Character.LowerTorso.Position, Vector3.new(0, -Character.UpperTorso.Size.y * 4.5, 0)), { Character, unpack(MainModule.Ignored) });
		local v29 = nil;
		if v28 and v28:IsDescendantOf(workspace.Players) then
			v29 = v28.Parent:FindFirstChild("Humanoid") or (v28.Parent.Parent:FindFirstChild("Humanoid") or v28.Parent.Parent.Parent:FindFirstChild("Humanoid"));
		end;
		if v29 then
			v18:Play(0.100000001, 1, 1.5);
			wait(0.75);
			MainRemoteEvent:FireServer("Stomp");
			wait(0.4);
		end;
		u11 = false;
	end;
end;
