	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Korblox.SwitchFrame.Switch.Position = UDim2.new(0.5, 0, 0, 0);
else

end
-- ==================== WEAPON SKINS GUI SYSTEM ====================
local u47 = false
local u48 = false
local function skinsDebounceActi()
	if u48 == false then
		u48 = true
		MainScreenGui.WeaponSkinsGUI.Visible = not MainScreenGui.WeaponSkinsGUI.Visible
		task.wait(0.01)
		if MainScreenGui.WeaponSkinsGUI.Visible then
			game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false)
		else
			game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)
		end
		----------------------------------------------------------------
		-- STOMP INJECTION (FRAMEWORK 2 ‚Üí FRAMEWORK 1)
		----------------------------------------------------------------

		task.spawn(function()
			print("[STOMPS] Inject start")

			local gui = MainScreenGui.WeaponSkinsGUI
			local contents =
				gui.Body.Wrapper.SkinView.Frame.Guns.Contents

			if not contents then
				warn("[STOMPS] Contents not found")
				return
			end

			local StompModule = game.ReplicatedStorage:WaitForChild("StompModule")

			-- Prevent double-injection
			if contents:FindFirstChild("STOMPS_HEADER") then
				print("[STOMPS] Already injected")
				return
			end

			-- Header
			local header = Instance.new("TextLabel")
			header.Name = "STOMPS_HEADER"
			header.Size = UDim2.new(1, 0, 0, 36)
			header.BackgroundTransparency = 1
			header.Text = "STOMPS"
			header.TextScaled = true
			header.TextColor3 = Color3.fromRGB(255,255,255)
			header.Parent = contents

			-- Old stomp button template
			local template = MainScreenGui.StompFrame:FindFirstChild("StompButtonTemplate")

			if not template then
				warn("[STOMPS] StompButtonTemplate missing")
				return
			end

			for _,mod in ipairs(StompModule:GetChildren()) do
				if mod:IsA("ModuleScript") then
					print("[STOMPS] Adding:", mod.Name)

					local btn = template:Clone()
					btn.Visible = true
					btn.Name = mod.Name
					btn.Text = mod.Name
					btn.Parent = contents

					btn.MouseButton1Click:Connect(function()
						print("[STOMPS] Selected:", mod.Name)
						game.ReplicatedStorage.MainRemoteEvent:FireServer(
							"SetStomp",
							mod.Name
						)
					end)
				end
			end
		end)

		task.wait(0.5)
		u48 = false
	end
end

local u50 = false
MainScreenGui.Crew.BottomLeft.Skins.Activated:Connect(function()
	skinsDebounceActi()
	if u50 == false then
		u50 = true
		local StompEffects = {}
		local v203 = require(game:GetService("ReplicatedStorage").SkinModules)
		local StompsModule = require(game:GetService("ReplicatedStorage").StompModule)
		local v204 = require(game:GetService("ReplicatedStorage").SkinModules.Icon)
		local l__GunModels__205 = game:GetService("ReplicatedStorage").SkinModules.GunModels
		local l__Knives__206 = game:GetService("ReplicatedStorage").SkinModules.Knives
		local l__TweenService__207 = game:GetService("TweenService")
		local l__WeaponSkinsGUI__208 = MainScreenGui:WaitForChild("WeaponSkinsGUI")
		local l__Wrapper__209 = l__WeaponSkinsGUI__208:WaitForChild("Body"):WaitForChild("Wrapper")
		local l__SkinView__210 = l__Wrapper__209:WaitForChild("SkinView")
		local l__CrateOpening__211 = l__Wrapper__209:WaitForChild("CrateOpening")
		local l__Guns__212 = l__SkinView__210:WaitForChild("Guns")
		----------------------------------------------------------------
		-- STOMPS SETUP (INJECT INTO WEAPON SKINS GUI)
		----------------------------------------------------------------

		local StompContainer = l__Guns__212:WaitForChild("Contents")

		-- Prevent duplicate load
		if not StompContainer:FindFirstChild("STOMPS_DIVIDER") then
			print("[STOMPS] Injecting stomps into WeaponSkinsGUI")

			-- Divider label
			local divider = Instance.new("TextLabel")
			divider.Name = "STOMPS_DIVIDER"
			divider.Size = UDim2.new(1, 0, 0, 36)
			divider.BackgroundTransparency = 1
			divider.Text = "STOMPS"
			divider.TextScaled = true
			divider.TextColor3 = Color3.fromRGB(255,255,255)
			divider.Parent = StompContainer

			for stompName, stompData in pairs(StompsModule) do
				print("[STOMPS] Adding:", stompName)

				-- Clone an existing gun button (safe)
				local template = StompContainer:FindFirstChildWhichIsA("ImageButton", true)
				if not template then
					warn("[STOMPS] No template button found")
					break
				end

				local btn = template:Clone()
				btn.Name = "STOMP_" .. stompName
				btn.Parent = StompContainer
				btn.Visible = true

				-- Label
				if btn:FindFirstChild("Name") then
					btn.Name.Text = stompName
				elseif btn:FindFirstChildWhichIsA("TextLabel") then
					btn:FindFirstChildWhichIsA("TextLabel").Text = stompName
				end

				-- Remove gun-only visuals safely
				if btn:FindFirstChild("GunModel") then
					btn.GunModel:Destroy()
				end

				-- Click logic
				btn.MouseButton1Click:Connect(function()
					print("[STOMPS] Selected:", stompName)
					MainRemoteEvent:FireServer("SetStomp", stompName)
				end)
			end
		end


		local function RarityColor(p66)
			if p66 == "Common" then
				return Color3.new(0.176471, 1, 0.164706)
			end
			if p66 == "Rare" then
				return Color3.new(0.807843, 0.109804, 0.109804)
			end
			if p66 == "Epic" then
				return Color3.new(0.419608, 0.243137, 1)
			end
			if p66 == "Legendary" then
				return Color3.new(1, 0.764706, 0.207843)
			end
			if p66 ~= "Exclusive" then
				return
			end
			return Color3.new(0.533333, 0.52549, 0.517647)
		end

		local l__Skins__51 = l__SkinView__210:WaitForChild("Skins")
		local function RarityGradient(Rarity)
			if Rarity == "Common" then
				return l__Skins__51.Contents.Rarities.Container.Common.UIGradient.Color
			end
			if Rarity == "Rare" then
				return l__Skins__51.Contents.Rarities.Container.Rare.UIGradient.Color
			end
			if Rarity == "Epic" then
				return l__Skins__51.Contents.Rarities.Container.Epic.UIGradient.Color
			end
			if Rarity ~= "Legendary" then
				if Rarity == "Exclusive" then
					return ColorSequence.new(Color3.new(1, 1, 1), Color3.new(0.890196, 0.839216, 0.0980392))
				else
					return
				end
			end
			return l__Skins__51.Contents.Rarities.Container.Legendary.UIGradient.Color
		end

