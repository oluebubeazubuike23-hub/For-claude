RunService.Heartbeat:connect(function()
	local v190 = 16;
	local v191 = 50;
	if not BodyEffects.Attacking.Value and Character:FindFirstChild("[Knife]") and not BodyEffects.Reload.Value  then
		v191 += 10
		v190 += 5
	end
	if BodyEffects.Movement:FindFirstChild("SuperSpeed") then
		v190 = 70;
	end;
	if BodyEffects.Movement:FindFirstChild("HighJump") then
		v191 = 75;
	end;
	if u13 or Character.Head:FindFirstChild("GOT_PEPPER_SPRAY") then
		v190 = v190 * 0.4;
	end;
	if Character.Head:FindFirstChild("GOT_PEPPER_SPRAY") then
		if MainScreenGui.PepperSpray.Visible == false then
			MainScreenGui.PepperSpray.Visible = true;
			game.Lighting.PepperSprayBlur.Enabled = true;
		end;
		local v192 = tick() / 2 % 1;
		MainScreenGui.PepperSpray.BackgroundTransparency = 0.6 + v192 * 0.2;
		MainScreenGui.PepperSpray.BackgroundColor3 = Color3.fromRGB(255, 33, 33):Lerp(Color3.fromRGB(255, 120, 75), v192);
		local v193 = tick() * 10;
		game.Lighting.PepperSprayBlur.Size = 8 + (math.floor(v193 / 6) % 2 == 0 and v193 % 6 or 6 - v193 % 6);
	elseif MainScreenGui.PepperSpray.Visible == true then
		MainScreenGui.PepperSpray.Visible = false;
		game.Lighting.PepperSprayBlur.Enabled = false;
	end;
	if Character.Humanoid.Health < 55 then
		v190 = v190 * 0.8;
	end;
	if BodyEffects.Grabbed.Value ~= nil then
		v190 = v190 - 4;
	end;
	if Character.Humanoid.FloorMaterial == Enum.Material.Snow and u35 == false and u13 == true then
		v186.Parent = Character;
	else
		v186.Parent = nil;
	end;
	if not u13 and u7 and Character.Humanoid.Health > 55 and Character.Humanoid.MoveDirection.magnitude > 0 and BodyEffects["K.O"].Value == false and BodyEffects.Movement:FindFirstChild("NoWalkSpeed") == nil and BodyEffects.Movement:FindFirstChild("ReduceWalk") == nil and (BodyEffects.Movement:FindFirstChild("NoJumping") == nil and BodyEffects:FindFirstChild("Block") == nil) then
		local v194 = false;
		if BodyEffects:findFirstChild("Symbiote") and BodyEffects.Symbiote.Value == false then
			v194 = true;
		end;
		if not v194 then
			v190 = v190 + 6;
			if BodyEffects.Movement:findFirstChild("SuperSpeed") then
				v190 = v190 + 30;
			end;
			if BodyEffects.Movement:findFirstChild("FastSprint") then
				v190 = v190 + 9;
			end;
		end;
	end;
	if BodyEffects:FindFirstChild("Block") then
		v190 = 5;
	end;
	if BodyEffects["K.O"].Value == true or BodyEffects.Movement:FindFirstChild("NoWalkSpeed") then
		v190 = 0;
	end;
	if BodyEffects.Movement:FindFirstChild("HulkJump") then
		v191 = 90;
	end;
	if BodyEffects["K.O"].Value == true or (BodyEffects.Movement:FindFirstChild("NoJumping") or u34) then
		v191 = 0
	end
	if BodyEffects.Movement:FindFirstChild("ReduceWalk") or BodyEffects.Reload.Value == true then
		v190 = v190 - 9;
	end;
	if Character:FindFirstChild("Parachute-BattleRoyale") then
		v190 = 65;
		if workspace.Gravity > 150 then
			workspace.Gravity = 5;
		end;
		if workspace:FindPartOnRayWithWhitelist(Ray.new(Character.HumanoidRootPart.Position, Vector3.new(0, -5 * Character.HumanoidRootPart.Size.y, 0)), { workspace.MAP }) then
			MainRemoteEvent:FireServer("RemoveParachute");
		end;
	elseif workspace.Gravity < 150 then
		workspace.Gravity = 196.2;
	end;
	local v195 = math.max(0, v190);
	_G.CurrentWS = v195;
	Character.Humanoid.WalkSpeed = v195;
	Character.Humanoid.JumpPower = math.max(0, v191);
	MainScreenGui.Bar.Energy.bar.Size = UDim2.new(BodyEffects.Defense.Value / 100 * 0.95, 0, 0.83, 0);
	MainScreenGui.Bar.Armor.bar.Size = UDim2.new(BodyEffects.Armor.Value / ReplicatedStorage.MaxArmor.Value * 0.95, 0, 0.83, 0);
	MainScreenGui.Bar.Armor.firebar.Size = UDim2.new(BodyEffects.FireArmor.Value / ReplicatedStorage.MaxArmor.Value * 0.95, 0, 0.83, 0);
	MainScreenGui.Bar.HP.bar.Size = UDim2.new(Character.Humanoid.Health / Character.Humanoid.MaxHealth * 0.95, 0, 0.83, 0);


	local currentHealth = math.floor(Character.Humanoid.Health)
	local maxHealth = math.floor(Character.Humanoid.MaxHealth)
	local maxArmor = math.floor(ReplicatedStorage.MaxArmor.Value)
	local currentArmor = math.floor(BodyEffects.Armor.Value)

	MainScreenGui.Bar.HP.TextLabel.Text = tostring(currentHealth .. "/" .. maxHealth)
	MainScreenGui.Bar.Armor.TextLabel.Text = tostring(currentArmor .. "/" .. maxArmor)

	if workspace:GetAttribute("Valentine") then
		MainScreenGui.Bar.HP.bar.BackgroundColor3 = Color3.fromRGB(182, 123, 179)
	end

	if u13 then
		if not v19.IsPlaying then
			v19:Play();
		end;
		if Character.Humanoid.MoveDirection.magnitude > 0 then
			v19:AdjustSpeed(1);
		else
			v19:AdjustSpeed(0);
		end;
	elseif v19.IsPlaying then
		v19:Stop();
	end;
	local v196 = MainModule.GunHold(Character);
	_G.HoldGunBool = v196;
	if v196 and BodyEffects:FindFirstChild("Block") == nil then
		local v197 = CurrentCamera.CFrame.p + CurrentCamera.CFrame.LookVector * 60;
		if _G.MobileShiftLock then
		else
			v197 = Mouse.Hit.p;
		end;
		MainRemoteEvent:FireServer("DEAHOODMOUSEPOSx3^3", v197);
		if MainModule.IsMobile() then
			MainScreenGui.ShootButton.Visible = true;
		end;
		if u10 then
			u36(true, nil);
		else
			u36(false, true);
		end;
		MainScreenGui.GunCombatToggle.Visible = true;
	else
		MainScreenGui.GunCombatToggle.Visible = false;
		MainScreenGui.ShootButton.Visible = false;
		u36(false);
	end;
	if tick() - _G.TimerMoney > 360 then
		_G.TimerMoney = tick();
		MainRemoteEvent:FireServer("TimerMoney");
	end;
	if LocalPlayer.DataFolder.Information:FindFirstChild("Jail") then
		if tonumber(LocalPlayer.DataFolder.Information.Jail.Value) > 0 then
			if (LocalPlayer.Character.HumanoidRootPart.Position - workspace.Ignored.JAILLOCATION1.Position).magnitude < ReplicatedStorage.JailRadius.Value then
				MainScreenGui.TimerJail.Text = "Jail Timer: " .. LocalPlayer.DataFolder.Information.Jail.Value;
			else
				MainScreenGui.TimerJail.Text = "Jail Timer: Stay in cage for countdown";
			end;
			MainScreenGui.TimerJail.Visible = true;
			if u40 == false then
				u40 = true;
				MainRemoteEvent:FireServer("TimerDecrease");
				delay(1, function()
					u40 = false;
				end);
			end;
		else
			MainScreenGui.TimerJail.Visible = false;
			if Character.BodyEffects.Cuff.Value == true then
				MainRemoteEvent:FireServer("UNCUFF");
			end;
		end;
	else
		MainScreenGui.TimerJail.Visible = false;
	end;
	if MainModule.IsMobile() then
		if not (not Character:FindFirstChild("Combat")) or not (not Character:FindFirstChild("[Pitchfork]")) or not (not Character:FindFirstChild("[Knife]")) or not (not Character:FindFirstChild("[Bat]")) or not (not Character:FindFirstChild("[Shovel]")) or not (not Character:FindFirstChild("[SledgeHammer]")) or Character:FindFirstChild("[StopSign]") or Character:FindFirstChild("[Katana]") then
			MainScreenGui.ChargeButton.Visible = true;
		else
			MainScreenGui.ChargeButton.Visible = false;
		end;
		if Character:FindFirstChild("[Taser]") then
			MainScreenGui.TaseButton.Visible = true;
		else
			MainScreenGui.TaseButton.Visible = false;
		end;
		MainScreenGui.Leaderboard.Visible = false;
		if u41 == false then
			u41 = true;
			StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, true);
		end;
	else
		MainScreenGui.Leaderboard.Size = UDim2.new(0, 200, 0, 320);
		MainScreenGui.Leaderboard.TextButton.Visible = false;
		if u41 then
			u41 = false;
			StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, false);
		end;
	end;
	if _G.Snowing and _G.Reduce_Lag ~= true then
		if _G.SnowingDebug == nil then
			_G.SnowingDebug = tick();
		end;
		local v198 = _G.SnowBlock;
		if v198 == nil then
			v198 = ReplicatedStorage.SnowBlock:Clone();
			v198.Parent = workspace.Ignored;
			_G.SnowBlock = v198;
		end;
		v198.CFrame = CFrame.new(CurrentCamera.CFrame.p) + Vector3.new(0, 10, 0) + CurrentCamera.CFrame.lookVector * Vector3.new(3, 0, 3);
		if tick() - _G.SnowingDebug > 0.8 then
			local l__p__199 = CurrentCamera.CFrame.p;
			local v200 = workspace:FindPartOnRayWithWhitelist(Ray.new(Vector3.new(l__p__199.x, 250, l__p__199.z), Vector3.new(0, l__p__199.y - 250, 0)), { workspace.MAP });
			if v200 and v200.Material ~= Enum.Material.Grass then
				v198.ParticleEmitter.Enabled = false;
				game.Lighting.SnowBlur.Enabled = false;
			else
				game.Lighting.SnowBlur.Enabled = true;
				v198.ParticleEmitter.Enabled = true;
			end;
			_G.SnowingDebug = tick();
		end;
	end;
end);



