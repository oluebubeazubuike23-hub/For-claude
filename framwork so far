function UpdateStompUnlock()
	local PlayerData = LocalPlayer.DataFolder
	local StompFrame = MainScreenGui:FindFirstChild("StompFrame")
	if not StompFrame then
		warn("[Stomp UI] Missing StompFrame UI!")
		return
	end

	-- Clear old
	for _, child in pairs(StompFrame:GetChildren()) do
		if child:IsA("TextButton") then
			child:Destroy()
		end
	end

	-- Read saved stomps list
	local stompsJSON = PlayerData.Information:FindFirstChild("SavedStomps")
	local stomps = {}
	if stompsJSON and stompsJSON.Value ~= "" then
		stomps = game:GetService("HttpService"):JSONDecode(stompsJSON.Value)
	end

	-- Create button for each stomp
	for _, stompName in ipairs(stomps) do
		local btn = Instance.new("TextButton")
		btn.Name = stompName .. "_btn"
		btn.Text = stompName
		btn.Size = UDim2.new(0, 120, 0, 30)
		btn.Parent = StompFrame

		btn.Activated:Connect(function()
			MainRemoteEvent:FireServer("EquipStompFX", stompName)
		end)
	end
end

MainRemoteEvent.OnClientEvent:Connect(function(action, ...)
	-- =====================
	-- STOMP FX
	-- =====================
	if action == "FX_KILL" then
		local stompName, victimHRP, attacker = ...

		if not victimHRP or not victimHRP.Parent then return end
		print("[CLIENT FX] Playing stomp:", stompName)

		local KillFX = ReplicatedStorage:WaitForChild("KillFX")

		local FXModule = KillFX:FindFirstChild(stompName)
		if not FXModule then
			warn("[CLIENT FX] Missing stomp FX:", stompName, "using Regular")
			FXModule = KillFX:FindFirstChild("Regular")
			if not FXModule then return end
		end

		local ok, fx = pcall(require, FXModule)
		if not ok then
			warn("[CLIENT FX] Failed to require:", FXModule.Name)
			return
		end

		if fx.RunStompFx then
			fx.RunStompFx(
				stompName,
				victimHRP,
				victimHRP.Parent,
				attacker,
				nil
			)
		elseif fx.Play then
			fx.Play(victimHRP.Parent, attacker)
		else
			warn("[CLIENT FX] No RunStompFx or Play in:", FXModule.Name)
		end

		return
	end

	-- =====================
	-- UPDATE STOMP
	-- =====================
	if action == "UpdateStomp" then
		UpdateStompUnlock()
		return
	end
	if action == "KILLED" then
		local Kills = MainScreenGui.Killed
		Kills.TextLabel.Text = "Killed ".. ...
		game:GetService("TweenService"):Create(Kills, TweenInfo.new(5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out),
			{
				BackgroundTransparency = 1
			}):Play()
		wait(5)
		Kills.Visible = false
	end
	if action == "DROPCASHALL" then
		MainRemoteEvent:FireServer("DROP_CASH_ALL")
	end
	if action == "MSG" then
		local v54;
		v54 = nil;
		if v54 == nil then
			v54 = MainScreenGui.StormMoving;
			v54.Visible = true;
			game:GetService("TweenService"):Create(v54, TweenInfo.new(1, Enum.EasingStyle.Linear),
				{
					Position = UDim2.new(0.8, 0, 0);
				}):Play();
			local l__Time__11;
			l__Time__11 = 4;
			repeat
				wait(1);
				l__Time__11 = l__Time__11 - 1;
				v54.TextLabel.Text = "Storm is moving in ".. l__Time__11.." seconds...";
			until l__Time__11 == 0;
			game:GetService("TweenService"):Create(v54, TweenInfo.new(0.5, Enum.EasingStyle.Linear),
				{
					Position = UDim2.new(0.976, 0, 0);
				}):Play();
			wait(0.5);
			v54.Visible = false;
			v54.Position = UDim2.new(0.976, 0, 0);
		end;
	end;
	if action == "ReportSent" then
		_G.Notification("Report Sent!", 0.7)
		return 
	end;
	if action == "Shout" then
		local v126 = Instance.new("TextLabel");
		v126.Size = UDim2.new(1, 0, 1, 0);
		v126.BackgroundColor3 = Color3.new(0.192157, 0.192157, 0.192157);
		v126.BackgroundTransparency = 0;
		v126.Text = ...;
		v126.TextColor3 = Color3.new(0.956863, 0.956863, 0.956863);
		v126.TextStrokeTransparency = 0;
		v126.TextStrokeColor3 = Color3.new(0.184314, 0.184314, 0.184314);
		v126.TextSize = 30;
		v126.ZIndex = 10;
