						return
					end
					u53 = StompsModule[v230].Crate
					if not u52 then
						return
					end
				else
					MainRemoteEvent:FireServer("EquipStompFX", tostring(v230))
					return
				end
				v215()
			end)

			table.insert(u60["Stomps"], v232)

			if StompsModule[v230].Rarity ~= "Exclusive" then
				if StompsModule[v230].Crate == nil then
					table.insert(v226.Stomps, {
						Name = v230,
						TextureID = StompsModule[v230].TemplateID,
						Rarity = StompsModule[v230].Rarity,
					})
				else
					if v226.CrateNumStomps[tostring(StompsModule[v230].Crate)] == nil then
						v226.CrateNumStomps[tostring(StompsModule[v230].Crate)] = {}
					end
					table.insert(v226.CrateNumStomps[tostring(StompsModule[v230].Crate)], {
						Name = v230,
						TextureID = v230.TemplateID,
						Rarity = StompsModule[v230].Rarity,
					})
				end
			end
		end

		table.insert(v220, v226)
		local v242 = l__Guns__212.Contents.GunEntry:Clone()
		v242.Visible = true
		v242.Container.Preview.Image = "rbxassetid://11785402571"
		v242.Container.Info.WeaponName.Text = "Stomps"
		v242.Container.Info.AvailableCount.Text = count1 .. " available"
		v242.Name = "Stomps"
		v242.Parent = l__Guns__212.Contents
		v242.Button.Activated:Connect(function()
			FixName2("Stomps")
		end)

		-- UPDATE FUNCTIONS
		function UpdateSkinUnlock()
			local l__DataFolder__243 = LocalPlayer:WaitForChild("DataFolder")
			local l__WeaponSkins__244 = l__DataFolder__243.Information:FindFirstChild("WeaponSkins")
			if l__WeaponSkins__244 then
				pcall(function()
					for v245, v246 in pairs(game:GetService('HttpService'):JSONDecode(l__WeaponSkins__244.Value)) do
						for v247, v248 in pairs(v246) do
							if v248 ~= false then
								l__Skins__51.Contents.Entries:FindFirstChild(v245 .. v247).Preview.LockImageLabel.Visible = false
							end
						end
					end
				end)
			end
			local l__EquipSkins__255 = l__DataFolder__243.Information:FindFirstChild("EquipSkins")
			if l__EquipSkins__255 then
				pcall(function()
					for v257, v258 in pairs(game:GetService("HttpService"):JSONDecode(l__EquipSkins__255.Value)) do
						for i,v in pairs(u60[v257]) do
							v.Preview.Equipped.Visible = false
						end
						if v258 ~= false then
							l__Skins__51.Contents.Entries:FindFirstChild(v257 .. v258).Preview.Equipped.Visible = true
						end
					end
				end)
			end
		end

		function UpdateStompUnlock()
			local l__DataFolder__243 = LocalPlayer:WaitForChild("DataFolder")
			local SavedStomps = l__DataFolder__243.Information:FindFirstChild("SavedStomps")
			if SavedStomps then
				pcall(function()
					for v245, v246 in pairs(game:GetService('HttpService'):JSONDecode(SavedStomps.Value)) do
						l__Skins__51.Contents.Entries:FindFirstChild(v246).Preview.LockImageLabel.Visible = false
					end
				end)
			end
			local StompFX = l__DataFolder__243.Information:FindFirstChild("StompFX")
			if StompFX then
				pcall(function()
					for i,v in pairs(u60["Stomps"]) do
						v.Preview.Equipped.Visible = false
					end
					l__Skins__51.Contents.Entries:FindFirstChild(StompFX.Value).Preview.Equipped.Visible = true
				end)
			end
		end

		UpdateStompUnlock()
		UpdateSkinUnlock()

		l__SkinView__210.Header.CloseButton.Activated:connect(function()
			skinsDebounceActi()
		end)
	end
end)
