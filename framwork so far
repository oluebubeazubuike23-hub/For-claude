		v183.Size = UDim2.new(1, 0, 1, 0)
		v183.BackgroundColor3 = Color3.new(0.192157, 0.192157, 0.192157)
		v183.BackgroundTransparency = 0
		v183.Text = p_u_182
		v183.TextColor3 = Color3.new(0.956863, 0.956863, 0.956863)
		v183.TextStrokeTransparency = 0
		v183.TextStrokeColor3 = Color3.new(0.184314, 0.184314, 0.184314)
		v183.TextSize = 30
		v183.ZIndex = 10
		v183.Font = Enum.Font.Oswald
		v183.Parent = MainScreenGui.Notified
		wait(1.6)
		game:GetService("TweenService"):Create(v183, TweenInfo.new(2, Enum.EasingStyle.Linear), {
			["TextTransparency"] = 1, 
			["TextStrokeTransparency"] = 1
		}):Play()
		wait(2)
		v183:Destroy()
	end)
end

