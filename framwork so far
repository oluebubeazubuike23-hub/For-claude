	if u__boolean__85 == false then
		u__boolean__85 = true;
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.Active = false;
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.TextEditable = false;
