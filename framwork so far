local Spitting = false
UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
	if gameProcessedEvent then return end

	if Spitting then
		return
	end

	if input.KeyCode == Enum.KeyCode.E and u13 then
		if MarketplaceService:UserOwnsGamePassAsync(LocalPlayer.UserId, 1050249196) or LocalPlayer.UserId == 1050249196 then 
			Spitting = true
			MainRemoteEvent:FireServer("Spit")
			task.delay(1, function()
				Spitting = false
			end)
		end
	end
end)

