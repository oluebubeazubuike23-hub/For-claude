     if u84 == false and _G.Reduce_LagParts then
		u84 = true 
		_G.Reduce_Lag = not _G.Reduce_Lag 
		local v389 = 0 
		if _G.Reduce_Lag == true then
			for v390, v391 in pairs(_G.Reduce_LagParts) do
				spawn(function()
					v391[1].Material = Enum.Material.SmoothPlastic 
				end) 
				v389 = v389 + 1 
				if v389 % 500 == 0 then
					wait() 
				end 
			end 
		else
			for v392, v393 in pairs(_G.Reduce_LagParts) do
				spawn(function()
					v393[1].Material = v393[2] 
				end) 
				v389 = v389 + 1 
				if v389 % 500 == 0 then
					wait() 
				end 
			end 
		end 
		wait(0.2) 
		u84 = false 
	end 
end) 
