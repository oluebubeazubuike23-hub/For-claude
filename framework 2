local Muted = false 
pcall(function()
	for i, v in pairs(workspace.Players:GetChildren()) do
		if v.LowerTorso:FindFirstChild("BOOMBOXSOUND") then
			v.LowerTorso.BOOMBOXSOUND.Volume = 0.5 
		end 
	end 
end) 
workspace.Players.DescendantAdded:Connect(function(Descendant)
	if Muted == true and Descendant and Descendant:IsA("Sound") and Descendant.Name == "BOOMBOXSOUND" and not Descendant:IsDescendantOf(Character) then
		Descendant.Volume = 0
	end
end)
MainScreenGui.Crew.Settings.Settings.Mute.TextButton.MouseButton1Click:Connect(function()
	Muted = true 
	for i, v in pairs(workspace.Players:GetChildren()) do
		pcall(function()
			if v.LowerTorso:FindFirstChild("BOOMBOXSOUND") and v ~= Character then
				v.LowerTorso.BOOMBOXSOUND.Volume = 0 
			end 
		end) 
	end 
end) 
LocalPlayer.Chatted:connect(function(Message)
	if Message:lower() == "/e stats" then
		MainScreenGui.StatsInformation.Visible = not MainScreenGui.StatsInformation.Visible 
		local MaxMuscle = game:GetService("ReplicatedStorage").MaxMuscle.Value 
		local MuscleInformation = LocalPlayer.DataFolder:WaitForChild("Information"):FindFirstChild("MuscleInformation") 
		local Muscle
		if MuscleInformation then
			Muscle = MuscleInformation.Value 
		else
			Muscle = 0 
		end 
		local MSize = (MaxMuscle + Muscle) / (MaxMuscle * 2) 
		MainScreenGui.StatsInformation.Muscle.Bar.Frame.BackgroundColor3 = Color3.fromRGB(255, 0, 0):lerp(Color3.fromRGB(174, 255, 149), MSize) 
		MainScreenGui.StatsInformation.Muscle.Bar.Frame.Size = UDim2.new(MSize, 0, 1, 0) 
		local BoxingValue = LocalPlayer.DataFolder.Information:FindFirstChild("BoxingValue") 
		local Boxing
		if BoxingValue then
			Boxing = BoxingValue.Value 
		else
			Boxing = 0 
		end 
		local BSize = Boxing / game:GetService("ReplicatedStorage").MaxBoxing.Value 
		MainScreenGui.StatsInformation.Boxing.Bar.Frame.BackgroundColor3 = Color3.fromRGB(255, 0, 0):lerp(Color3.fromRGB(174, 255, 149), BSize) 
		MainScreenGui.StatsInformation.Boxing.Bar.Frame.Size = UDim2.new(BSize, 0, 1, 0) 
	end 
end) 
