					if MainScreenGui.BattleRoyale.Visible == false then
						MainScreenGui.BattleRoyale.Visible = true 
						Character.HumanoidRootPart.CFrame = v195:Lerp(v196, 0.5) 
					end 
					_G.BattleRoyaleTeam1 = _G.BattleRoyaleTeam1 or game:GetService("GroupService"):GetGroupInfoAsync(game:GetService("ReplicatedStorage").BattleRoyale.Team1.Value).EmblemUrl 
					_G.BattleRoyaleTeam2 = _G.BattleRoyaleTeam2 or game:GetService("GroupService"):GetGroupInfoAsync(game:GetService("ReplicatedStorage").BattleRoyale.Team2.Value).EmblemUrl 
					MainScreenGui.BattleRoyale.Team1.Image = _G.BattleRoyaleTeam1 
					MainScreenGui.BattleRoyale.Team2.Image = _G.BattleRoyaleTeam2 
					local Waiting1 = 0 
					local Waiting2 = 0
					for i, v in pairs(game.Players:GetChildren()) do
						pcall(function()
							if v and v:FindFirstChild("DataFolder") and v.DataFolder.Information:FindFirstChild("Crew") then
								local ID = tonumber(v.DataFolder.Information.Crew.Value) 
								if ID == game:GetService("ReplicatedStorage").BattleRoyale.Team1.Value then
									Waiting1 += 1 
									if MainScreenGui.BattleRoyale.Team1Player:FindFirstChild(v.Name) == nil then
										AddBattleRoyaleTextToParent(v, MainScreenGui.BattleRoyale.Team1Player) 
										return 
									end 
								elseif ID == game:GetService("ReplicatedStorage").BattleRoyale.Team2.Value then
									Waiting2 += 1 
									if MainScreenGui.BattleRoyale.Team2Player:FindFirstChild(v.Name) == nil then
										AddBattleRoyaleTextToParent(v, MainScreenGui.BattleRoyale.Team2Player) 
									end 
								end 
							end 
						end) 
					end 
					MainScreenGui.BattleRoyale.TextLabel.Text = tostring(Waiting1) .. "-WAITING-" .. tostring(Waiting2) 
					return 
				end 
				MainScreenGui.BattleRoyale.Visible = false 
				MainScreenGui.BattleRoyaleAlives.Visible = true 
				local Count1 = 0 
				local Count2 = 0
				for i, v in pairs(game.Players:GetChildren()) do
					pcall(function()
						if v and v.Character and v.Character.BodyEffects.Dead.Value ~= true and v.DataFolder.Information:FindFirstChild("Crew") then
							if tonumber(v.DataFolder.Information.Crew.Value) == game:GetService("ReplicatedStorage").BattleRoyale.Team1.Value then
								Count1 += 1 
								return 
							end 
							if tonumber(v.DataFolder.Information.Crew.Value) == game:GetService("ReplicatedStorage").BattleRoyale.Team2.Value then
								Count2 += 1 
							end 
						end 
					end) 
				end 
				MainScreenGui.BattleRoyaleAlives.Team1.Image = _G.BattleRoyaleTeam1 
				MainScreenGui.BattleRoyaleAlives.Team2.Image = _G.BattleRoyaleTeam2 
				MainScreenGui.BattleRoyaleAlives.Team1.TextLabel.Text = 0 
				MainScreenGui.BattleRoyaleAlives.Team2.TextLabel.Text = 0 
				if Count1 == 0 and game:GetService("ReplicatedStorage").BattleRoyale.CTF.Value == false then
					if BRWon == false then
						BRWon = true 
						BattleRoyaleWinner(2) 
					end 
				elseif Count2 == 0 and game:GetService("ReplicatedStorage").BattleRoyale.CTF.Value == false and BRWon == false then
					BRWon = true 
					BattleRoyaleWinner(1) 
				end 
				if workspace.Ignored:FindFirstChild("HELICOPTER1") and workspace.Ignored:FindFirstChild("HELICOPTER2") then
					CurrentCamera.CameraType = Enum.CameraType.Custom 
					if tonumber(LocalPlayer.DataFolder.Information.Crew.Value) == game:GetService("ReplicatedStorage").BattleRoyale.Team1.Value then
						CurrentCamera.CameraSubject = workspace.Ignored.HELICOPTER1.PrimaryPart 
					else
						CurrentCamera.CameraSubject = workspace.Ignored.HELICOPTER2.PrimaryPart 
					end 
				elseif BodyEffects.Dead.Value == false then
					if u42 == false then
						u42 = true 
						CurrentCamera.CameraSubject = Character.Humanoid 
					end 
				else
					MainScreenGui.BattleRoyaleAlives.Next.Visible = true 
					MainScreenGui.BattleRoyaleAlives.Back.Visible = true 
					MainScreenGui.BattleRoyaleAlives.Spectate.Visible = true 
					local v202 = game.Players:GetChildren()[u40 % #game.Players:GetChildren() + 1] 
					CurrentCamera.CameraSubject = v202.Character.Humanoid 
					MainScreenGui.BattleRoyaleAlives.Spectate.Text = v202.Name 
				end 
			end 
		end 
	end) 
end) 
