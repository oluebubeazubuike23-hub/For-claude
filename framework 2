	if EditCooldown == false and Main ~= false then
		EditCooldown = true 
		MainEvent:FireServer("PlaceHouseItem", Main[1], Angle) 
		wait(0.1) 
		EditCooldown = false 
	end 
end) 

MainScreenGui.EditFrame.Rotate.TextButton.MouseButton1Click:connect(function()
	if EditCooldown == false then
		EditCooldown = true 
		Angle = (Angle + 90) % 360 
		wait(0.1) 
		EditCooldown = false 
	end 
end) 

