Table = {
	timer = tick(), 
	times = 0, 
	disabling = false
} 

LocalPlayer.Backpack.ChildAdded:connect(function(p52)
	if p52:IsA("Tool") then
		if tick() - Table.timer < 0.1 then
			Table.times = Table.times + 1 
		else
			Table.times = 0 
		end 
		if Table.times >= 4 and Table.disabling == false then
			Table.disabling = true 
			game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false) 
			delay(1, function()
				Table.disabling = false 
				game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true) 
			end) 
		end 
		Table.timer = tick() 
	end 
end)

BodyEffects.ChildAdded:Connect(function(Child)
	if Child.Name == "Block" then
		Blocking(true) 
	end 
end)

BodyEffects.ChildRemoved:Connect(function(Child)
	if Child.Name == "Block" then
		Blocking(false) 
	end 
end) 

