local function BattleRoyaleWinner(Team)
	local TeamImage
	if Team == 1 then
		TeamImage = _G.BattleRoyaleTeam1 
	else
		TeamImage = _G.BattleRoyaleTeam2 
	end 
	MainScreenGui.BattleRoyaleAlives.VictoryRoyale.Image = TeamImage 
	MainScreenGui.BattleRoyaleAlives.VictoryRoyale.Size = UDim2.new(0, 0, 0, 0) 
	MainScreenGui.BattleRoyaleAlives.VictoryRoyale.Visible = true 
	game:GetService("TweenService"):Create(MainScreenGui.BattleRoyaleAlives.VictoryRoyale, TweenInfo.new(0.75, Enum.EasingStyle.Bounce, Enum.EasingDirection.In, 0, false, 0), {
		Size = UDim2.new(0, 200, 0, 200)
	}):Play() 
	MainScreenGui.BattleRoyaleAlives.VictoryRoyale.VictoryMusic:Play() 
	delay(15, function()
		game:GetService("TweenService"):Create(MainScreenGui.BattleRoyaleAlives.VictoryRoyale, TweenInfo.new(5), {
			ImageTransparency = 1
		}):Play() 
		game:GetService("TweenService"):Create(MainScreenGui.BattleRoyaleAlives.VictoryRoyale.Trophy, TweenInfo.new(5), {
			ImageTransparency = 1
		}):Play() 
	end) 
end 
local u24 = {} 
local u25 = { "19323823", "19323854" } 
local u26 = { "19003948", "19003957", "19003971", "19003978", "19003982", "19003990", "19003998" } 
MainEvent.OnClientEvent:connect(function(p59, ...)
	if p59 == "ShotFrom" then
