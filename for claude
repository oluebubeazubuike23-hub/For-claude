local FOVButton = MainScreenGui.Crew.Settings.Frame.InnerFrame.List.FOV.TextButton
local FOVInput = MainScreenGui.Crew.Settings.Frame.InnerFrame.List.FOV.IDBox

local savedFOV = LocalPlayer:GetAttribute("CameraFOV")
if savedFOV and typeof(savedFOV) == "number" then
	CurrentCamera.FieldOfView = math.clamp(savedFOV, 70, 120)
end

FOVButton.Activated:Connect(function()
	local input = tonumber(FOVInput.Text)
	if input then
		local clamped = math.clamp(input, 70, 120)
		CurrentCamera.FieldOfView = clamped
		LocalPlayer:SetAttribute("CameraFOV", clamped)
		SaveFOV:FireServer(clamped)
	end
end)
