local SaveFOV = ReplicatedStorage:WaitForChild("SaveFOV")
local FOVStore = DataStoreService:GetDataStore("PlayerFOVs")

Players.PlayerAdded:Connect(function(player)
	local success, result = pcall(function()
		return FOVStore:GetAsync(player.UserId)
	end)

	if success and typeof(result) == "number" then
		player:SetAttribute("CameraFOV", math.clamp(result, 70, 120))
	else
		player:SetAttribute("CameraFOV", 70)
	end
end)

SaveFOV.OnServerEvent:Connect(function(player, fov)
	if typeof(fov) == "number" and fov >= 70 and fov <= 120 then
		player:SetAttribute("CameraFOV", fov)
		pcall(function()
			FOVStore:SetAsync(player.UserId, fov)
		end)
	end
end)

-- Webhook URLs
local KickWebhookUrl = "https://discord.com/api/webhooks/1386478807787245628/NqW1yS0pwJyOtTmFPMjr0OYypChWVjEUbNQk729vYzl9DX4Fm0ssxqdeu062Cy3dPMC2"

