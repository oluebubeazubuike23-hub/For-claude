	if v309:FindFirstChild("Highlight") then
		v309.Highlight:Destroy()
	end
	if v309:FindFirstChild("ClickDetector") then
		local lClickDetector310 = v309.ClickDetector
		lClickDetector310.MouseHoverEnter:Connect(function()
			local v311 = Instance.new("Highlight", v309)
			v311.DepthMode = Enum.HighlightDepthMode.Occluded
			v311.FillTransparency = 1
			v311.OutlineTransparency = 1
			v311.OutlineColor = Color3.fromRGB(0)
		end)
		lClickDetector310.MouseHoverLeave:Connect(function()
			if v309:FindFirstChild("Highlight") then
				v309.Highlight:Destroy()
			end
		end)
	end
end

