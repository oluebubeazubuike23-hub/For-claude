		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Korblox.SwitchFrame.Switch.Position = UDim2.new(0.5, 0, 0, 0);
	else
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Korblox.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109);
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Korblox.SwitchFrame.Switch.Position = UDim2.new(0, 0, 0, 0);
	end
end);

local pairs_result1_33, pairs_result2_12, pairs_result3_39 = pairs(MainScreenGui.Crew.BottomLeft.Frame:GetChildren())
for _, v_32 in pairs_result1_33, pairs_result2_12, pairs_result3_39 do
	if v_32.Name ~= v_32:IsA("ImageButton") and not v_32:IsA("UIListLayout") then
		v_32.ImageTransparency = 1
		local ImageLabel_upvr = Instance.new("ImageLabel")
		ImageLabel_upvr.BackgroundTransparency = 1
		ImageLabel_upvr.Image = v_32.Image
		ImageLabel_upvr.Size = UDim2.new(1, 0, 1, 0)
		ImageLabel_upvr.Parent = v_32
		local var1691_upvw = false
		v_32.MouseEnter:Connect(function()
			if not var1691_upvw then
				var1691_upvw = true
				game:GetService("TweenService"):Create(ImageLabel_upvr, TweenInfo.new(0.25), {
					Position = UDim2.new(0, 0, -0.25, 0);
				}):Play()
			end
		end)
		v_32.MouseLeave:Connect(function()
			if var1691_upvw then
				var1691_upvw = false
				game:GetService("TweenService"):Create(ImageLabel_upvr, TweenInfo.new(0.25), {
					Position = UDim2.new(0, 0, 0, 0);
				}):Play()
			end
		end)
	end
end


local u80 = game:GetService("MarketplaceService")
function findImageDecal(p86)
	local u81 = nil;
	local u82 = p86;
	pcall(function()
		u81 = u80:GetProductInfo(u82);
	end);
	if not u81 then
		return false;
	end;
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.Icon.Image = "rbxassetid://6646175684";
	while true do
		if p86 - u82 < 20 then

		else
			break;
		end;
		local u83 = nil;
		pcall(function()
			u83 = u80:GetProductInfo(u82);
		end);
		if u83 then
			if u83.AssetTypeId == 1 then
				if u81.Creator.Id == u83.Creator.Id then
					return p86;
				end;
			end;
		end;
		u82 = u82 - 1;
		if (p86 - u82) % 25 == 0 then
			task.wait();
		end;
		if (p86 - u82) % 2 == 0 then
			MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.Icon.BackgroundColor3 = Color3.new(1, 0, 0);
		else
			MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.Icon.BackgroundColor3 = Color3.fromRGB(70, 70, 70);
		end;	
	end;
	return false;
end;
u79 = nil;
local u84 = nil;
function CursorImageCheck()
	return tonumber(u84);
end;
local u__boolean__85 = false;
local function UpdateCursorIcon(p87)
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.Icon.BackgroundColor3 = Color3.fromRGB(70, 70, 70);
	if not p87 or not tonumber(p87) then
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.Text = "Enter a valid Image-ID";
		u79 = nil;
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.Icon.Image = "rbxassetid://6312476440";
		return;
	end;
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.Text = p87;
	u79 = p87;
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.Icon.Image = "rbxassetid://" .. p87;
	MainScreenGui.Aim.CursorImage.Image = "rbxassetid://" .. u84;
end;
MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.FocusLost:connect(function()
	if u__boolean__85 == false then
		u__boolean__85 = true;
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.Active = false;
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.TextEditable = false;
		local l__Text__400 = MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.Text;
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.Text = "Searching: " .. l__Text__400;
		if tonumber(l__Text__400) then
			UpdateCursorIcon((findImageDecal(tonumber(l__Text__400))));
		else
			UpdateCursorIcon(nil);
		end;
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.Active = true;
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.IDBox.TextEditable = true;
		u__boolean__85 = false;
	end;
end);
MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.TextButton.Activated:connect(function()
	if u__boolean__85 == false then
		u__boolean__85 = true;
		MainRemoteEvent:FireServer("UpdateCursorImage", u79);
		u__boolean__85 = false;
	end;
end);
MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Crosshair.Reset.Activated:connect(function()
	if u__boolean__85 == false then
		u__boolean__85 = true;
		MainRemoteEvent:FireServer("ResetCursorImage", u79);
		u__boolean__85 = false;
	end;
end);
task.spawn(function()
	LocalPlayer.DataFolder:WaitForChild("CursorImage");
	u84 = LocalPlayer.DataFolder.CursorImage.Value;
	UpdateCursorIcon(LocalPlayer.DataFolder.CursorImage.Value);
	LocalPlayer.DataFolder.CursorImage.Changed:connect(function()
		u84 = LocalPlayer.DataFolder.CursorImage.Value;
		UpdateCursorIcon(LocalPlayer.DataFolder.CursorImage.Value);
	end);
end);

MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Stats.SwitchFrame.TextButton.Activated:Connect(function()
	MainScreenGui.StatsInformation.Visible = not MainScreenGui.StatsInformation.Visible;
	pcall(function()
		local l__Value__402 = ReplicatedStorage.MaxMuscle.Value;
		local l__MuscleInformation__403 = LocalPlayer.DataFolder:WaitForChild("Information"):FindFirstChild("MuscleInformation");
		local v404 = l__MuscleInformation__403.Value;
		if l__MuscleInformation__403 then
		else
			v404 = 0;
		end;
		local v405 = (l__Value__402 + v404) / (l__Value__402 * 2);
		MainScreenGui.StatsInformation.Muscle.Bar.Frame.BackgroundColor3 = Color3.fromRGB(109, 109, 109):lerp(Color3.fromRGB(109, 109, 109), v405);
		MainScreenGui.StatsInformation.Muscle.Bar.Frame.Size = UDim2.new(v405, 0, 1, 0);
		local l__BoxingValue__406 = LocalPlayer.DataFolder.Information:FindFirstChild("BoxingValue");
		local v407 = l__BoxingValue__406.Value;		
		if l__BoxingValue__406 then
		else
			v407 = 0;
		end;
		local v408 = v407 / ReplicatedStorage.MaxBoxing.Value;
		MainScreenGui.StatsInformation.Boxing.Bar.Frame.BackgroundColor3 = Color3.fromRGB(109, 109, 109):lerp(Color3.fromRGB(109, 109, 109), v408);
		MainScreenGui.StatsInformation.Boxing.Bar.Frame.Size = UDim2.new(v408, 0, 1, 0);
	end)
end)

