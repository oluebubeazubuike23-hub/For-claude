		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Headless.SwitchFrame.Switch.Position = UDim2.new(0, 0, 0, 0);
	end
end);

local KorbloxT = false

MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Korblox.SwitchFrame.TextButton.Activated:Connect(function()
	KorbloxT = not KorbloxT;
	MainRemoteEvent:FireServer("ChangeSettings", "Korblox", KorbloxT)

	if KorbloxT then
