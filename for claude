for name, data in pairs(GamepassButtons) do
	local button = data.Button
	local gamepassId = data.GamepassId

	if button then
		button.MouseButton1Click:Connect(function()
			MarketplaceService:PromptGamePassPurchase(Player, gamepassId)
		end)

		MarketplaceService.PromptGamePassPurchaseFinished:Connect(function(plr, purchasedPassId, wasPurchased)
			if plr == Player and purchasedPassId == gamepassId and wasPurchased then
				_G.Notification("Thanks for buying " .. name .. "! Your item will be delivered shortly! ")

				LogRemote:FireServer(name, purchasedPassId)
			end	
		end)
	end
end

function PromptGP(Item, Text, Bool)
	local AAA = Instance.new("BindableEvent")
	MainRemoteEvent:FireServer("GamePurchase", LocalPlayer.Name, InfoModule[Item].GamepassId, InfoModule[Item].ProductId, Bool, Item)
	AAA:Fire()
	AAA.Event:Wait()
	AAA:Destroy()
end

MainRemoteEvent.OnClientEvent:Connect(function(Subject,...)
	if Subject:lower() == 'notification' then
		_G.Notification(...)
	end
end)


if LocalPlayer.DataFolder.Information.MuteBoombox.Value == true then
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.BoomBox.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109);
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.BoomBox.SwitchFrame.Switch.Position = UDim2.new(0.5, 0, 0, 0);
	for v328, v329 in pairs(workspace.Players:GetChildren()) do
		pcall(function()
			if v329.LowerTorso:FindFirstChild("BOOMBOXSOUND") and v329 ~= Character then
				v329.LowerTorso.BOOMBOXSOUND.Volume = 0
			end
		end)
	end
else

end

task.spawn(function()
	if LocalPlayer.DataFolder.Information.LowGFX.Value == true then
		if _G.Reduce_Lag then
			return
		end
		_G.Reduce_Lag = true
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.LowGFX.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109);
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.LowGFX.SwitchFrame.Switch.Position = UDim2.new(0.5, 0, 0, 0);

		local needsChange = false
		for _, Part in pairs(_G.Reduce_LagParts) do
			if not Part[1]:IsDescendantOf(workspace.Players) then
				if Part[1].Material ~= Enum.Material.SmoothPlastic then
					needsChange = true
					break
				end
			end
		end

		if not needsChange then
			return
		end

		local Partsa = 0
		for _, Part in pairs(_G.Reduce_LagParts) do
			if not Part[1]:IsDescendantOf(workspace.Players) then
				task.spawn(function()
					if Part[1].Material ~= Enum.Material.SmoothPlastic then
						Part[1].Material = Enum.Material.SmoothPlastic
					end
				end)
				Partsa = Partsa + 1
				if Partsa % 500 == 0 then
					task.wait()
				end
			end
		end
	end
end)

MainScreenGui.Promotion.Close.Activated:Connect(function()
	MainScreenGui.Promotion.Visible = false
end)

if LocalPlayer.DataFolder.Information.Headless.Value == true then
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Headless.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109);
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.Headless.SwitchFrame.Switch.Position = UDim2.new(0.5, 0, 0, 0);
else

end

