end

LocalPlayer.PlayerGui.ChildAdded:connect(function(p91)
	wait(0.5);
	if p91.Name ~= "ProximityPrompts" and p91.Name ~= "ContextActionGui" and p91.Name ~= "ControlGui" and p91.Name ~= "GoThruGui" then
		local u89 = " / extra information: ";
		local u90 = 0;
		local function u91(p92)
			if not (not p92:IsA("TextLabel")) or not (not p92:isA("TextBox")) or p92:IsA("TextButton") then
				u89 = u89 .. " / " .. p92.Text;
				u90 = u90 + 1;
			end;
			for v431, v432 in pairs(p92:GetChildren()) do
				u91(v432);
			end;
		end;
		u91(p91);
		if u90 > 0 then
			MainRemoteEvent:FireServer("GUI_CHECK", p91, p91.Name .. u89);
		end;
	end;
end);
MainScreenGui.Crew.Settings.Frame.CloseButton.MouseButton1Click:connect(function()
	MainScreenGui.Crew.Settings.Visible = not MainScreenGui.Crew.Settings.Visible
end)
local v_u_823 = false
if _G.Reduce_Lag then
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.LowGFX.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109)
	MainScreenGui.Crew.Settings.Frame.InnerFrame.List.LowGFX.SwitchFrame.Switch.Position = UDim2.new(109, 109, 109)
end
local isToggling = false 
MainScreenGui.Crew.Settings.Frame.InnerFrame.List.LowGFX.SwitchFrame.TextButton.MouseButton1Click:Connect(function()
	if not isToggling and _G.Reduce_LagParts then
		isToggling = true
		_G.Reduce_Lag = not _G.Reduce_Lag
		MainRemoteEvent:FireServer("ChangeSettings", "LowGFX", _G.Reduce_Lag)

		local processedParts = 0
		if _G.Reduce_Lag then
			local SnowBlock = _G.SnowBlock
			if _G.Snowing and SnowBlock then
				SnowBlock.Parent = nil
			end
			for _, partData in pairs(_G.Reduce_LagParts) do
				spawn(function()
					if partData[1].Material ~= Enum.Material.Snow then
						partData[1].Material = Enum.Material.SmoothPlastic
					end
				end)
				processedParts = processedParts + 1
				if processedParts % 500 == 0 then
					wait()
				end
			end
			MainScreenGui.Crew.Settings.Frame.InnerFrame.List.LowGFX.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109)
			MainScreenGui.Crew.Settings.Frame.InnerFrame.List.LowGFX.SwitchFrame.Switch.Position = UDim2.new(0.5, 0, 0, 0)
		else
			local SnowBlock = _G.SnowBlock
			if _G.Snowing and SnowBlock then
				SnowBlock.Parent = workspace.Ignored
			end
			for _, partData in pairs(_G.Reduce_LagParts) do
				spawn(function()
					partData[1].Material = partData[2]
				end)
				processedParts = processedParts + 1
				if processedParts % 500 == 0 then
					wait()
				end
			end
			MainScreenGui.Crew.Settings.Frame.InnerFrame.List.LowGFX.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109)
			MainScreenGui.Crew.Settings.Frame.InnerFrame.List.LowGFX.SwitchFrame.Switch.Position = UDim2.new(0, 0, 0, 0)
		end

		wait(0.2)
		isToggling = false
	end
end)


MainScreenGui.Crew.Settings.Frame.InnerFrame.List.AntiMacroFling.SwitchFrame.TextButton.Activated:Connect(function()
	local AntiFlingButton = MainScreenGui.Crew.Settings.Frame.InnerFrame.List.AntiMacroFling.SwitchFrame.TextButton
	AntiFlingButton.Active = false

	local Player = game.Players.LocalPlayer
	local DataFolder = Player:FindFirstChild("DataFolder")
	if not DataFolder then
		warn(">> Datafolder not found for player.")
		AntiFlingButton.Active = true
		return
	end

	local Information = DataFolder:FindFirstChild("Information")
	if not Information then
		warn(">> Data information not found for player.")
		AntiFlingButton.Active = true
		return
	end

	local AntiFling = Information:FindFirstChild("AntiFling")
	if not AntiFling then
		AntiFling = Instance.new("BoolValue")
		AntiFling.Name = "AntiFling"
		AntiFling.Value = false
		AntiFling.Parent = Information
	end

	AntiFling.Value = not AntiFling.Value
	MainRemoteEvent:FireServer("ChangeSettings", "AntiFling")

	if AntiFling.Value then
		local Character = Player.Character or Player.CharacterAdded:Wait()
		local Humanoid = Character:WaitForChild("Humanoid")
		Humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
		Humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
		Humanoid:SetStateEnabled(Enum.HumanoidStateType.GettingUp, false)

		task.delay(0.01, function()
			AntiFlingButton.Active = true
			MainScreenGui.Crew.Settings.Frame.InnerFrame.List.AntiMacroFling.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109)
			MainScreenGui.Crew.Settings.Frame.InnerFrame.List.AntiMacroFling.SwitchFrame.Switch.Position = UDim2.new(0.5, 0, 0, 0)
		end)
	else
		local Character = Player.Character or Player.CharacterAdded:Wait()
		local Humanoid = Character:WaitForChild("Humanoid")
		Humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, true)
		Humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, true)
		Humanoid:SetStateEnabled(Enum.HumanoidStateType.GettingUp, true)

		task.delay(1, function()
			AntiFlingButton.Active = true
			MainScreenGui.Crew.Settings.Frame.InnerFrame.List.AntiMacroFling.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109)
			MainScreenGui.Crew.Settings.Frame.InnerFrame.List.AntiMacroFling.SwitchFrame.Switch.Position = UDim2.new(0, 0, 0, 0)
		end)
	end
end)

