-- Stomp FX System
function CheckForFX(stompName, victimHRP, attackerPlayer)
	local success, fxModule = pcall(function()
		return require(ReplicatedStorage.KillFX[stompName])
	end)

	if success and fxModule and fxModule.RunStompFx then
		pcall(function()
			fxModule.RunStompFx(stompName, victimHRP, victimHRP.Parent, LocalPlayer, {})
		end)
	end
end

