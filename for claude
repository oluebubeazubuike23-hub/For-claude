StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, false)
local function v_u_682(p_u_670)
	local v_u_671 = MainScreenGui.Leaderboard.PlayerScroll.CloneExample:Clone()
	v_u_671.Name = p_u_670.UserId
	v_u_671.Visible = true
	v_u_671.PlayerName.Text = p_u_670.Name
	task.spawn(function()
		p_u_670:WaitForChild("leaderstats")
		p_u_670.leaderstats:WaitForChild("Wanted")
		local v672 = v_u_671.PlayerWanted
		local v673 = p_u_670.leaderstats.Wanted.Value
		v672.Text = math.abs(v673)
		local v_u_676 = p_u_670.leaderstats.Wanted.Changed:connect(function()
			local v674 = v_u_671.PlayerWanted
			local v675 = p_u_670.leaderstats.Wanted.Value
			v674.Text = math.abs(v675)
		end)
		p_u_670:WaitForChild("DataFolder")
		p_u_670.DataFolder:WaitForChild("Officer")
		local function v_u_678()
			local v677
			if p_u_670.DataFolder.Officer.Value == 0 then
				v_u_671.PlayerName.TextColor3 = Color3.new(1, 1, 1)
				v_u_671.PlayerName.TextStrokeColor3 = Color3.new(0, 0, 0)
				v677 = MainModule.GroupImage(p_u_670)
			else
				v_u_671.PlayerName.TextColor3 = Color3.new(0, 0.333333, 1)
				v_u_671.PlayerName.TextStrokeColor3 = Color3.new(1, 1, 1)
				v677 = "rbxassetid://5078542968"
			end
			if v677 then
				v_u_671.ImageLabel.Image = v677
				v_u_671.ImageLabel.Visible = true
			else
				v_u_671.ImageLabel.Visible = false
			end
		end
		v_u_678()
		local v_u_679 = p_u_670.DataFolder.Officer.Changed:connect(function()
			v_u_678()
		end)
		local v_u_680 = p_u_670.CharacterAdded:connect(function()
			v_u_678()
		end)
		local v681 = Instance.new("BindableEvent")
		v681.Name = "bindableevent"
		v681.Event:Connect(function()
			v_u_676:Disconnect()
			v_u_679:Disconnect()
			v_u_680:Disconnect()
			v_u_671:Destroy()
		end)
		v681.Parent = v_u_671
		v_u_671.Parent = MainScreenGui.Leaderboard.PlayerScroll
	end)
end
v_u_682(LocalPlayer)
for _, v683 in pairs(game.Players:GetPlayers()) do
	if v683 ~= LocalPlayer then
		v_u_682(v683)
	end
end
game.Players.PlayerAdded:Connect(function(p684)
	v_u_682(p684)
end)
game.Players.PlayerRemoving:Connect(function(p685)
	local v686 = MainScreenGui.Leaderboard.PlayerScroll:FindFirstChild(p685.UserId)
	if v686 then
		v686:WaitForChild("bindableevent")
		v686.bindableevent:Fire()
	end
end)
local v896 = MainScreenGui.Crew.BottomLeft.Frame.Crew
local v897 = MainScreenGui.Crew.CrewJoin.CloseJoin
closeTop25Button = MainScreenGui.Crew.Top50.CloseButton
local v898 = MainScreenGui.Crew.Current.CloseCurrent
local v899 = MainScreenGui.Crew.Current.LeaveCrew
local v_u_900 = MainScreenGui.Crew.ImageLabel.Image
local v_u_901 = MainScreenGui.Crew.CrewJoin
local v_u_902 = MainScreenGui.Crew.Current
function openCrewUI()
	if MainModule.CheckInCrew(LocalPlayer) then
		updateCrewStuff()
		v_u_901.Visible = false
		v_u_902.Visible = true
		v_u_902.Parent.TopButton.Visible = true
		v_u_902.Parent.CurrentButton.Visible = true
		v_u_901.Parent.Top50.Visible = false
	else
		v_u_901.Visible = true
		v_u_902.Visible = false
		v_u_902.Parent.TopButton.Visible = false
		v_u_902.Parent.CurrentButton.Visible = false
		v_u_901.Parent.Top50.Visible = false
	end
end
function closeCrewUI()
	v_u_901.Visible = false
	v_u_902.Visible = false
	v_u_902.Parent.TopButton.Visible = false
	v_u_902.Parent.CurrentButton.Visible = false
	v_u_901.Parent.Top50.Visible = false
end
function updateCrewStuff()
	v_u_901.Visible = false
	v_u_902.Visible = true
	v_u_902.Parent.TopButton.Visible = true
	v_u_902.Parent.CurrentButton.Visible = true
	local v_u_903 = v_u_902.ScrollingFrame
	for _, v904 in ipairs(v_u_903:GetChildren()) do
		if v904:IsA("Frame") then
			v904:Destroy()
		end
	end
	for _, v_u_905 in ipairs(game:GetService("Players"):GetPlayers()) do
		pcall(function()
			if MainModule.CheckTeam(LocalPlayer, v_u_905, true) then
				local v906 = v_u_902.TemplateHolder:Clone()
				v906.Visible = true
				local v907 = v906.Template
				v907.Visible = true
				v907.PlrName.Text = v_u_905.Name
				v907.Icon.Image = "rbxthumb://type=AvatarHeadShot&id=" .. v_u_905.UserId .. "&w=150&h=150"
				v906.Parent = v_u_903
			end
		end)
	end
	local v908 = MainModule.GetGroupInfo(LocalPlayer)
	if v908 then
		v_u_902.GroupIcon.Image = v908.EmblemUrl
		local v909 = v_u_902.Frame.GroupID
		local v910 = v908.Id
		v909.Text = tostring(v910)
		v_u_902.Frame.GroupName.Text = v908.Name
	else
		v_u_902.GroupIcon.Image = v_u_900
		v_u_902.Frame.GroupID.Text = ""
		v_u_902.Frame.GroupName.Text = ""
	end
end
local v911 = MainModule.GetGroupInfo(LocalPlayer)
if v911 then
	v_u_902.GroupIcon.Image = v911.EmblemUrl
	local v912 = v_u_902.Frame.GroupID
	local v913 = v911.Id
	v912.Text = tostring(v913)
	v_u_902.Frame.GroupName.Text = v911.Name
else
	v_u_902.GroupIcon.Image = v_u_900
	v_u_902.Frame.GroupID.Text = ""
	v_u_902.Frame.GroupName.Text = ""
end
v896.MouseButton1Click:Connect(function()
	if v_u_901.Visible or v_u_902.Visible then
		closeCrewUI()
	else
		openCrewUI()
	end
end)
v_u_902.Parent.TopButton.MouseButton1Click:Connect(function()
	v_u_902.Visible = false
	v_u_901.Parent.Top50.Visible = true
end)
v_u_902.Parent.CurrentButton.MouseButton1Click:Connect(function()
	v_u_902.Visible = true
	v_u_901.Parent.Top50.Visible = false
end)
v897.MouseButton1Click:Connect(function()
	closeCrewUI()
end)
closeTop25Button.MouseButton1Click:Connect(function()
	closeCrewUI()
end)
v898.MouseButton1Click:Connect(function()
	closeCrewUI()
end)
v899.MouseButton1Click:Connect(function()
	if MainModule.CheckInCrew(LocalPlayer) then
		MainRemoteEvent:FireServer("LeaveCrew")
		v_u_902.GroupIcon.Image = v_u_900
		v_u_902.Frame.GroupID.Text = ""
		v_u_902.Frame.GroupName.Text = ""
		closeCrewUI()
		v_u_901.Visible = true
	end
end)
v_u_901.TypeHolder.TextBox.FocusLost:Connect(function(p914)
	if p914 and not MainModule.CheckInCrew(LocalPlayer) then
		MainRemoteEvent:FireServer("JoinCrew", v_u_901.TypeHolder.TextBox.Text)
	end
end)
local function v924()
	local v915 = v_u_901.ScrollingFrame
	local v916 = v915.Template
	v916.Visible = false
	for _, v917 in ipairs(v915:GetChildren()) do
		if v917:IsA("Frame") and v917 ~= v916 then
			v917:Destroy()
		end
	end
	local v918, v919 = pcall(function()
		return game:GetService("GroupService"):GetGroupsAsync(LocalPlayer.UserId)
	end)
	if v918 and v919 then
		for _, v_u_920 in ipairs(v919) do
			local v921 = v916:Clone()
			v921.Parent = v915
			v921.Visible = true
			v921.GroupIcon.Image = "rbxthumb://type=GroupIcon&id=" .. v_u_920.Id .. "&w=150&h=150"
			v921.GroupName.Text = v_u_920.Name
			local v922 = v921.ID
			local v923 = v_u_920.Id
			v922.Text = tostring(v923)
			v921.ImageButton.MouseButton1Click:Connect(function()
				MainRemoteEvent:FireServer("JoinCrew", v_u_920.Id)
			end)
		end
	end
end
v_u_901.Refresh.MouseButton1Click:Connect(v924)
v924()
