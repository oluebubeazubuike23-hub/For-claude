AnimationPackDebounce = false
local l__ScrollingFrame__390 = MainScreenGui.AnimationPack.ScrollingFrame;
for v391, v392 in pairs(l__ScrollingFrame__390:GetChildren()) do
	if v392:IsA("TextButton") then
