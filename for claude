MainScreenGui.Crew.Settings.Frame.InnerFrame.List.BoomBox.SwitchFrame.TextButton.Activated:Connect(function()
	u80 = not u80;
	MainRemoteEvent:FireServer("ChangeSettings", "MuteBoombox", u80)

	if u80 then
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.BoomBox.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109);
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.BoomBox.SwitchFrame.Switch.Position = UDim2.new(0.5, 0, 0, 0);
	else
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.BoomBox.SwitchFrame.Switch.BackgroundColor3 = Color3.new(109, 109, 109);
		MainScreenGui.Crew.Settings.Frame.InnerFrame.List.BoomBox.SwitchFrame.Switch.Position = UDim2.new(0, 0, 0, 0);
	end

	if u80 == true then
		for v328, v329 in pairs(workspace.Players:GetChildren()) do
			pcall(function()
				if v329.LowerTorso:FindFirstChild("BOOMBOXSOUND") and v329 ~= Character then
					v329.LowerTorso.BOOMBOXSOUND.Volume = 0
				end;
			end);
		end;
	else
		for v330, v331 in pairs(workspace.Players:GetChildren()) do
			pcall(function()
				if v331.LowerTorso:FindFirstChild("BOOMBOXSOUND") and v331 ~= Character then
					v331.LowerTorso.BOOMBOXSOUND.Volume = 0.5;
				end;
			end);
		end;
	end
end);

local ColorCorrectionT = false

MainScreenGui.Crew.Settings.Frame.InnerFrame.List.ColorCorrection.SwitchFrame.TextButton.Activated:Connect(function()
	ColorCorrectionT = not ColorCorrectionT
	MainRemoteEvent:FireServer("ChangeSettings", "ColorCorrection", ColorCorrectionT)

	local switch = MainScreenGui.Crew.Settings.Frame.InnerFrame.List.ColorCorrection.SwitchFrame.Switch
	local uiStroke = switch:FindFirstChild("UIStroke")

	if ColorCorrectionT then
		switch.BackgroundColor3 = Color3.new(109, 109, 109)
		switch.Position = UDim2.new(0.5, 0, 0, 0)
		if uiStroke then
			uiStroke.Color = Color3.new(109, 109, 109)
		end

		local startSaturation = Lighting.ColorCorrection.Saturation
		local endSaturation = 1
		local duration = 1
		local elapsedTime = 0

		while elapsedTime < duration do
			local alpha = elapsedTime / duration
			Lighting.ColorCorrection.Saturation = startSaturation + (endSaturation - startSaturation) * alpha
			elapsedTime = elapsedTime + game:GetService("RunService").Heartbeat:Wait()
		end
		Lighting.ColorCorrection.Saturation = endSaturation

	else
		switch.BackgroundColor3 = Color3.new(109, 109, 109)
		switch.Position = UDim2.new(0, 0, 0, 0)
		if uiStroke then
			uiStroke.Color = Color3.new(109, 109, 109)
		end

		local startSaturation = Lighting.ColorCorrection.Saturation
		local endSaturation = 0.1
		local duration = 1
		local elapsedTime = 0

		while elapsedTime < duration do
			local alpha = elapsedTime / duration
			Lighting.ColorCorrection.Saturation = startSaturation + (endSaturation - startSaturation) * alpha
			elapsedTime = elapsedTime + game:GetService("RunService").Heartbeat:Wait()
		end
		Lighting.ColorCorrection.Saturation = endSaturation
	end
end)

