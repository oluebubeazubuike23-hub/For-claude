--framework 2
local MainScreenGui = game:GetService("StarterGui").MainScreenGui
local InventoryOpenCooldown = false
local InventoryModule = require(game.ReplicatedStorage.SkinModules)
local IconModule = require(game.ReplicatedStorage.SkinModules.Icon)
local LoadedEntries = {}
local CanOpenCrate = true
local WeaponSkinsGUI = MainScreenGui:WaitForChild("WeaponSkinsGUI");
local Wrapper = WeaponSkinsGUI:WaitForChild("Body"):WaitForChild("Wrapper");
local SkinView = Wrapper:WaitForChild("SkinView");
local Skins = SkinView:WaitForChild("Skins");
local GunModels = game.ReplicatedStorage.SkinModules.GunModels
local PurchaseFrame = WeaponSkinsGUI:WaitForChild("PurchaseFrame");
local LocalPlayer = game.Players.LocalPlayer
local BodyEffects = game:GetService("StarterPlayer").StarterCharacterScripts.BodyEffects.Dead
function UpdateInventoryStuff()
	local InventoryData = game:GetService("HttpService"):JSONDecode(LocalPlayer.DataFolder.Information.InventoryData.Value)
	local EquippedData = game:GetService("HttpService"):JSONDecode(LocalPlayer.DataFolder.Information.EquippedData.Value)
	for _, v in pairs(MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Skins.Contents.Entries:GetChildren()) do
		if v:IsA("Frame") and v.Name ~= "SkinEntryTemplate" then
			v.Preview.LockImageLabel.Visible = true
			v.Preview.Equipped.Visible = false
			if (InventoryData[MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Title.Text] and InventoryData[MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Title.Text][v.Name] and InventoryData[MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Title.Text][v.Name] == true) then
				v.Preview.LockImageLabel.Visible = false
			end
			if EquippedData[MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Title.Text] then
				if v.Name == EquippedData[MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Title.Text] then
					v.Preview.Equipped.Visible = true
				end
			end
		end
	end
end

local PurchaseAnimate = false


local function OpenCrate(Case,V2,Drop,ChoosenGun)
	MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Visible = false
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Crate.ViewportFrame:ClearAllChildren()
	local WorldModel = Instance.new("WorldModel")
	WorldModel.Parent = MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Crate.ViewportFrame
	local Crate = game.ReplicatedStorage.SkinModules.Crate:Clone()
	Crate.Parent = WorldModel
	local Camera = Instance.new("Camera")
	Camera.CFrame = CFrame.new(Crate.HumanoidRootPart.Position + Crate.HumanoidRootPart.CFrame.LookVector * 5 + Crate.HumanoidRootPart.CFrame.UpVector * 4, Crate.HumanoidRootPart.Position)
	Camera.Parent = MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Crate.ViewportFrame
	Camera.FieldOfView = 50
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Crate.ViewportFrame.CurrentCamera = Camera
	local IdleAnimation = Crate.AnimationController.Animator:LoadAnimation(Crate.AnimationController.IdleAnimation)
	local OpenAnimation = Crate.AnimationController.Animator:LoadAnimation(Crate.AnimationController.OpenAnimation)
	IdleAnimation:Play()
	local SpinSound = script.SpinSound:Clone()
	SpinSound.Parent = script
	SpinSound.Volume = 0
	SpinSound:Play()
	local SpinSoundTween = game:GetService("TweenService"):Create(SpinSound, TweenInfo.new(0.8, Enum.EasingStyle.Linear), {
		Volume = 0.5
	})
	SpinSoundTween:Play()
	local Items = {}
	local Item = nil
	--V2["UnlockCost"] = nil
	--V2["UnlockCostx10"] = nil
	--V2["UnlockProductId"] = nil
	--V2["UnlockProductIdx10"] = nil
	local hello = 0
	for i = 1,30 do
		hello = hello + 1
		local RandomKey = nil
		local RandomThing = nil
		local RandomGun = nil
		local Guns = {};
		local Skinss = {};
		for i,_ in pairs(InventoryModule) do
			table.insert(Guns,i);
		end;
		RandomGun = Guns[math.random(#Guns)];

		for i,_ in pairs(InventoryModule[RandomGun]) do
			if i.Crate == nil then
				table.insert(Skinss,i);
			end
		end;
		if i == 18 then
			RandomKey = Drop
			RandomThing = InventoryModule[RandomGun][Drop]
		else
			RandomKey = Skinss[math.random(#Skinss)];
			RandomThing = InventoryModule[RandomGun][RandomKey];
		end
		local SkinEntry = MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Entries.Container.Contents.SkinEntryTemplate:Clone()
		SkinEntry.Visible = true
		if not RandomThing["Rarity"] then
			LocalPlayer:Kick("Error happend while opening crate. Sorry! Your money are going to be back.")
		end
		SkinEntry.SkinInfo.Container.Rarity.Text = RandomThing["Rarity"]

		if RandomThing.Rarity == "Common" then
			SkinEntry.UIStroke.UIGradient.Color = ColorSequence.new(Color3.new(0.176471, 1, 0.164706),Color3.new(0.176471, 1, 0.164706));
			SkinEntry.SkinInfo.Container.Rarity.TextColor3 = Color3.new(0.176471, 1, 0.164706)
		end;
		if RandomThing.Rarity == "Rare" then
			SkinEntry.UIStroke.UIGradient.Color = ColorSequence.new(Color3.new(0.807843, 0.109804, 0.109804), Color3.new(0.807843, 0.109804, 0.109804));
			SkinEntry.SkinInfo.Container.Rarity.TextColor3 = Color3.new(0.807843, 0.109804, 0.109804)
		end;
		if RandomThing.Rarity == "Epic" then
			SkinEntry.UIStroke.UIGradient.Color = ColorSequence.new(Color3.new(0.419608, 0.243137, 1), Color3.new(0.419608, 0.243137, 1))
			SkinEntry.SkinInfo.Container.Rarity.TextColor3 = Color3.new(0.419608, 0.243137, 1)
		end;
		if RandomThing.Rarity == "Legendary" then
			SkinEntry.UIStroke.UIGradient.Color = ColorSequence.new(Color3.new(1, 0.764706, 0.207843),Color3.new(1, 0.764706, 0.207843));
			SkinEntry.SkinInfo.Container.Rarity.TextColor3 = Color3.new(1, 0.764706, 0.207843)
		end;
		local v232 = Instance.new("ViewportFrame");
		v232.Size = UDim2.new(1, 0, 1, 0);
		v232.BackgroundTransparency = 0;
		v232.Ambient = Color3.new(1, 1, 1);
		v232.LightColor = Color3.new(1, 1, 1);
		if hello == 18 then
			local v233 = GunModels[ChoosenGun]:Clone();
			v233.Parent = v232;
			local v234 = v233:IsA("Model") and v233.PrimaryPart or v233;
			v234.TextureID = InventoryModule[ChoosenGun][Drop].TextureID;
			local v235 = Instance.new("Camera");
			v235.CFrame = CFrame.new(v234.CFrame.p + Vector3.new(0.9, 0.9, 0.9) * v234.Size.magnitude * 0.4, v234.CFrame.p);
			v235.Parent = v232;
			v232.CurrentCamera = v235;
			v232.BackgroundTransparency = 1;
			v232.Parent = SkinEntry.Preview.Frame;
			SkinEntry.Parent = Skins.Contents.Entries
			local u61 = false;
			local u62 = v234.CFrame:ToObjectSpace(CFrame.new(v234.CFrame.p + Vector3.new(0.9, 0.9, 0.9) * v234.Size.magnitude * 0.4, v234.CFrame.p));
			hello = 0
		else
			local v233 = GunModels[RandomGun]:Clone();
			v233.Parent = v232;
			local v234 = v233:IsA("Model") and v233.PrimaryPart or v233;
			v234.TextureID = RandomThing.TextureID;
			local v235 = Instance.new("Camera");
			v235.CFrame = CFrame.new(v234.CFrame.p + Vector3.new(0.9, 0.9, 0.9) * v234.Size.magnitude * 0.4, v234.CFrame.p);
			v235.Parent = v232;
			v232.CurrentCamera = v235;
			v232.BackgroundTransparency = 1;
			v232.Parent = SkinEntry.Preview.Frame;
			SkinEntry.Parent = Skins.Contents.Entries
			local u61 = false;
			local u62 = v234.CFrame:ToObjectSpace(CFrame.new(v234.CFrame.p + Vector3.new(0.9, 0.9, 0.9) * v234.Size.magnitude * 0.4, v234.CFrame.p));
		end

		--SkinEntry.SkinInfo.Container.Rarity.TextColor3 = RandomThing["Color"]
		--SkinEntry.Preview.ImageLabel.Image = RandomThing["TextureID"]
		--SkinEntry.Preview.ImageLabel.ImageColor3 = RandomThing["Color"]
		SkinEntry.SkinInfo.Container.SkinName.Text = RandomKey
		--SkinEntry.UIStroke.UIGradient.Color = ColorSequence.new(RandomThing["Color"],RandomThing["Color"])
		--if RandomThing["Exclusive"] and RandomThing["Exclusive"] == true then
		--	SkinEntry.Limited.Visible = true
		--end
		SkinEntry.Parent = MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Entries.Container.Contents
		table.insert(Items,SkinEntry)
		if i == 18 then
			Item = SkinEntry
		end
	end

	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Visible = true
	local WaitTime = 0
	local Skipped = false
	local SkipConnection = MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Skip.Activated:Connect(function()
		Skipped = true
	end)
	local SizeTween = game:GetService("TweenService"):Create(MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Entries.Container.Contents, TweenInfo.new(3, Enum.EasingStyle.Quad), {
		Position = UDim2.new(0, -MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Entries.Container.Contents.AbsoluteSize.X * 0.5 + Item.AbsoluteSize.X * 0.5, 0, 0)
	})
	SizeTween:Play()
	while WaitTime < 3 and Skipped == false do
		WaitTime += task.wait()
	end
	SkipConnection:Disconnect()
	if Skipped then
		SizeTween:Cancel()
	else
		task.wait(0.6)
	end
	if SpinSoundTween.PlaybackState == Enum.PlaybackState.Playing then
		SpinSoundTween:Cancel()
	end
	game:GetService("TweenService"):Create(SpinSound, TweenInfo.new(0.4, Enum.EasingStyle.Linear), {
		Volume = 0
	}):Play()
	game:GetService("TweenService"):Create(MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Entries, TweenInfo.new(0.4, Enum.EasingStyle.Linear), {
		Size = UDim2.new(1, 0, 0, 0)
	}):Play()
	game:GetService("TweenService"):Create(MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Crate, TweenInfo.new(0.4, Enum.EasingStyle.Linear), {
		Size = UDim2.new(1, 0, 0.822, 0)
	}):Play()
	task.wait(0.4)
	SpinSound:Destroy()
	pcall(function()
		game:GetService("TweenService"):Create(Camera, TweenInfo.new(0.75, Enum.EasingStyle.Bounce), {
			FieldOfView = 70, 
			CFrame = CFrame.new(Crate.HumanoidRootPart.Position + Crate.HumanoidRootPart.CFrame.LookVector * 8 + Crate.HumanoidRootPart.CFrame.UpVector * 8, Crate.HumanoidRootPart.Position)
		}):Play()
	end)
	IdleAnimation:Stop()
	OpenAnimation:Play()
	task.wait(0.5)
	local v283 = GunModels[ChoosenGun]:Clone();
	v283.Parent = WorldModel;
	local v284 = v283:IsA("Model") and v283.PrimaryPart or v283;
	v284.TextureID = InventoryModule[ChoosenGun][Drop].TextureID;
	local l__Size__285 = v284.Size;
	v284.Size = Vector3.new(0, 0, 0);
	v284.CFrame = Crate.HumanoidRootPart.CFrame;
	task.wait(0.5)
	TweenService:Create(v284, TweenInfo.new(0.5, Enum.EasingStyle.Linear), {
		CFrame = v284.CFrame + Vector3.new(0, 4, 0), 
		Size = l__Size__285
	}):Play();
	script.OpenSound:Play()
	task.wait(0.5)
	OpenAnimation:AdjustSpeed(0)
	task.wait(0.3);
	TweenService:Create(Camera, TweenInfo.new(0.2, Enum.EasingStyle.Linear), {
		CFrame = CFrame.new((v284.CFrame * CFrame.new(0, 0, -l__Size__285.magnitude)).p, v284.Position)
	}):Play();
	local idk = tostring(Drop)
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.ATitle.Text = string.gsub(string.gsub(ChoosenGun .. " " .. idk, "%[", ""), "%]", "");

	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Skip.Text = "CLOSE"
	--MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Unboxed.SkinEntryTemplate.Visible = true
	game:GetService("TweenService"):Create(MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Unboxed.SkinEntryTemplate,TweenInfo.new(0.5),{Size = UDim2.new(0.274,0,0.725,0)}):Play()
	for _, v in pairs(Crate:GetChildren()) do
		if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" then
			game:GetService("TweenService"):Create(v, TweenInfo.new(0.4, Enum.EasingStyle.Linear), {
				Transparency = 1
			}):Play()
		end
	end
	task.wait(0.2)
	local Closed = false
	local CloseConnection = MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Skip.Activated:Connect(function()
		Closed = true
	end)
	while Closed == false do
		v284.CFrame = v284.CFrame * CFrame.fromAxisAngle(Vector3.new(0, 1, 0), math.rad(1));
		task.wait()
	end
	CloseConnection:Disconnect()
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Visible = false
	for i = 1, #Items do
		Items[i]:Destroy()
	end
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Unboxed.SkinEntryTemplate.Visible = false
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Skip.Text = "SKIP"
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.ATitle.Text = "Unboxing Crate Skins"
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.ATitle.TextColor3 = Color3.new(1, 1, 1)
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Entries.Size = UDim2.new(1, 0, 0.474, 0)
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Crate.Size = UDim2.new(1, 0, 0.348, 0)
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Entries.Visible = true
	MainScreenGui.WeaponSkinsGUI.Body.CrateOpening.Entries.Container.Contents.Position = UDim2.new(0, 0, 0, 0)
	MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Visible = true
end

local function PurchaseFrame(Crate)
	if InventoryModule[MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Title.Text] then
		local Clicked = false
		if Crate == "Unlock" then
			MainScreenGui.WeaponSkinsGUI.PurchaseFrame.TextLabel.Text = "Purchase & Open\nCrate (100K)"
		elseif Crate == "Unlock10x" then
			MainScreenGui.WeaponSkinsGUI.PurchaseFrame.TextLabel.Text = "Purchase & Open\n10x Crate (1M)"
		end
		MainScreenGui.WeaponSkinsGUI.PurchaseFrame.Confirm1Button.Visible = false
		MainScreenGui.WeaponSkinsGUI.PurchaseFrame.Confirm2Button.Visible = false
		MainScreenGui.WeaponSkinsGUI.PurchaseFrame.ConfirmButton.Visible = true
		MainScreenGui.WeaponSkinsGUI.PurchaseFrame.Visible = true
		script.ClickSound:Play()

		local PurchaseConnection = MainScreenGui.WeaponSkinsGUI.PurchaseFrame.ConfirmButton.Activated:Connect(function()
			Clicked = true
			game.ReplicatedStorage.MainEvent:FireServer("PurchaseSkinCrate",{InventoryModule[MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Title.Text]},Crate)
		end)
		local CancelConnection = MainScreenGui.WeaponSkinsGUI.PurchaseFrame.CancelButton.Activated:Connect(function()
			Clicked = true
		end)
		repeat task.wait() until Clicked == true
		if CancelConnection then
			CancelConnection:Disconnect()
		end
		if PurchaseConnection then
			PurchaseConnection:Disconnect()
		end
		MainScreenGui.WeaponSkinsGUI.PurchaseFrame.Visible = false
	end
end


local v219 = {};
local u58 = false;
local u59 = nil;
local u60 = {};

local function LoadInventory(V1)
	if V1 == false then
		local InventoryData = game:GetService("HttpService"):JSONDecode(LocalPlayer.DataFolder.Information.InventoryData.Value)
		local CodesData = game:GetService("HttpService"):JSONDecode(LocalPlayer.DataFolder.Information.CodesData.Value)
		local EquippedData = game:GetService("HttpService"):JSONDecode(LocalPlayer.DataFolder.Information.EquippedData.Value)
		game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.Backpack,false)
		for i_, v in pairs(IconModule) do
			if not table.find(LoadedEntries,i_) then
				local GunEntry = MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Guns.Contents.GunEntry:Clone()
				GunEntry.Button.Activated:Connect(function()
					MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Unlock.Text = "OPEN (100K)"
					MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Unlock10x.Text = "10X (1M)"
					MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Title.Text = i_
					MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Unlock10x.Visible = true
					MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Unlock.Visible = true
					--SkinView.Header.Unlock.Activated:Connect(function()
					--	is100k = true
					--	PurchaseFramee()
					--end)
					--SkinView.Header.Unlock10x.Activated:Connect(function()
					--	is100k = false
					--	PurchaseFramee()
					--end)
					for _, v in pairs(MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Guns.Contents:GetChildren()) do
						if v:IsA("Frame") then
							v["UIStroke"].Enabled = false
						end
					end
					GunEntry.UIStroke.Enabled = true
					for _, v in pairs(MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Skins.Contents.Entries:GetChildren()) do
						if v:IsA("Frame") and v.Name ~= "SkinEntryTemplate" then
							v:Destroy()
						end
					end
					for i, v in pairs(InventoryModule[i_]) do
						if i ~= "UnlockCostx10" and i ~= "UnlockCost" and i ~= "UnlockProductId" and i ~= "UnlockProductIdx10" then
							u60[v] = {};
							local SkinEntry = MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Skins.Contents.Entries.SkinEntryTemplate:Clone()
							SkinEntry.Name = i
							table.insert(u60[v], SkinEntry);
							local v232 = Instance.new("ViewportFrame");
							v232.Size = UDim2.new(1, 0, 1, 0);
							v232.BackgroundTransparency = 0;
							v232.Ambient = Color3.new(1, 1, 1);
							v232.LightColor = Color3.new(1, 1, 1);
							local v233 = GunModels[i_]:Clone();
							v233.Parent = v232;
							local v234 = v233:IsA("Model") and v233.PrimaryPart or v233;
							v234.TextureID = v.TextureID;
							local v235 = Instance.new("Camera");
							v235.CFrame = CFrame.new(v234.CFrame.p + Vector3.new(0.9, 0.9, 0.9) * v234.Size.magnitude * 0.4, v234.CFrame.p);
							v235.Parent = v232;
							v232.CurrentCamera = v235;
							v232.BackgroundTransparency = 1;
							v232.Parent = SkinEntry.Preview.Frame;
							SkinEntry.Parent = Skins.Contents.Entries;
							local u61 = false;
							local u62 = v234.CFrame:ToObjectSpace(CFrame.new(v234.CFrame.p + Vector3.new(0.9, 0.9, 0.9) * v234.Size.magnitude * 0.4, v234.CFrame.p));
							SkinEntry.MouseEnter:Connect(function()
								u61 = true;
								while u61 do
									v235.CFrame = v235.CFrame:Lerp(v234.CFrame * CFrame.Angles(0, tick() * 0.5 % math.pi * 2, 0) * u62, 0.25);
									task.wait();					
								end;
								v235.CFrame = CFrame.new(v234.CFrame.p + Vector3.new(0.9, 0.9, 0.9) * v234.Size.magnitude * 0.4, v234.CFrame.p);
							end);
							SkinEntry.MouseLeave:Connect(function()
								u61 = false;
							end);
							local ImageLabel = Instance.new("ImageLabel")
							if v.Crate == nil then
								ImageLabel.Image = "rbxassetid://5743022869"
							else
								ImageLabel.Image = "rbxassetid://9546076937"
							end
							local v237 = "rbxassetid://5743022869";
							ImageLabel.Name = "LockImageLabel"
							ImageLabel.Size = UDim2.new(0.4, 0, 0.4, 0)
							ImageLabel.BackgroundTransparency = 1
							ImageLabel.Position = UDim2.new(0.6, 0, 0.6, 0)
							ImageLabel.ImageColor3 = Color3.new(0.807843, 0.203922, 0.203922)
							ImageLabel.Parent = SkinEntry.Preview
							local Equipped = Instance.new("TextLabel")
							Equipped.Name = "Equipped"
							Equipped.Text = "E"
							Equipped.TextColor3 = Color3.new(0.321569, 1, 0.490196)
							Equipped.TextStrokeTransparency = 0
							Equipped.TextStrokeColor3 = Color3.new(0, 0, 0)
							Equipped.Size = UDim2.new(0.4, 0, 0.4, 0)
							Equipped.Position = UDim2.new(0.6, 0, 0.6, 0)
							Equipped.Font = Enum.Font.Oswald
							Equipped.TextScaled = true
							Equipped.BackgroundTransparency = 1
							Equipped.Visible = false
							Equipped.Parent = SkinEntry.Preview
							SkinEntry.Visible = true
							--SkinEntry.Preview.ImageLabel.Image = v.TextureID
							SkinEntry.SkinInfo.Container.Rarity.Text = v.Rarity
							SkinEntry.SkinInfo.Container.SkinName.Text = i
							--if v.Color then
							--	SkinEntry.Preview.ImageLabel.ImageColor3 = v.Color
							--	SkinEntry.SkinInfo.Container.Rarity.TextColor3 = v.Color
							--	SkinEntry.UIStroke.UIGradient.Color = ColorSequence.new(v.Color,v.Color)
							--end
							--if v.Exclusive and v.Exclusive == true or v.Exclusive == false then
							--	SkinEntry.Limited.Visible = true
							--end

							if v.Rarity == "Common" then
								SkinEntry.UIStroke.UIGradient.Color = ColorSequence.new(Color3.new(0.176471, 1, 0.164706),Color3.new(0.176471, 1, 0.164706));
								SkinEntry.SkinInfo.Container.Rarity.TextColor3 = Color3.new(0.176471, 1, 0.164706)
							end;
							if v.Rarity == "Rare" then
								SkinEntry.UIStroke.UIGradient.Color = ColorSequence.new(Color3.new(0.807843, 0.109804, 0.109804), Color3.new(0.807843, 0.109804, 0.109804));
								SkinEntry.SkinInfo.Container.Rarity.TextColor3 = Color3.new(0.807843, 0.109804, 0.109804)
							end;
							if v.Rarity == "Epic" then
								SkinEntry.UIStroke.UIGradient.Color = ColorSequence.new(Color3.new(0.419608, 0.243137, 1), Color3.new(0.419608, 0.243137, 1))
								SkinEntry.SkinInfo.Container.Rarity.TextColor3 = Color3.new(0.419608, 0.243137, 1)
							end;
							if v.Rarity == "Legendary" then
								SkinEntry.UIStroke.UIGradient.Color = ColorSequence.new(Color3.new(1, 0.764706, 0.207843),Color3.new(1, 0.764706, 0.207843));
								SkinEntry.SkinInfo.Container.Rarity.TextColor3 = Color3.new(1, 0.764706, 0.207843)
							end;
							if v.Rarity ~= "Exclusive" then

							end;

							SkinEntry.Button.Activated:Connect(function()
								if ImageLabel.Visible == false and v.Crate == nil then
									MainEvent:FireServer("EquipWeaponSkins",i_,i)
								elseif ImageLabel.Visible == true and v.Crate == nil then
									PurchaseFrame("Unlock")
								end
							end)
							SkinEntry.Parent = MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Skins.Contents.Entries
						end
					end
					UpdateInventoryStuff()
				end)
				GunEntry.Name = i_
				GunEntry.Container.Info.WeaponName.Text = i_
				table.insert(LoadedEntries,i_) 
				GunEntry.Container.Preview.Image = v
				GunEntry.Parent = MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Guns.Contents
				GunEntry.Visible = true
			end
		end
		for _,v in pairs(MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Guns.Contents:GetChildren()) do
			if v.Name ~= "GunEntry" and v:IsA("Frame") then
				local Amount = 0
				for i, v in pairs(InventoryModule[v.Name]) do
					if i ~= "UnlockCostx10" and i ~= "UnlockCost" and i ~= "UnlockProductId" and i ~= "UnlockProductIdx10" then
						Amount += 1
					end
				end
				if InventoryData[v.Name] then
					for i_, v_ in pairs(InventoryData[v.Name]) do
						if (v_ == true) then
							Amount += -1
						end
					end 
				end
				v.Container.Info.AvailableCount.Text = Amount .. " available"
			end
		end
		UpdateInventoryStuff()
		script.Parent.MainScreenGui.WeaponSkinsGUI.Visible = true
	else
		game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.Backpack,true)
		script.Parent.MainScreenGui.WeaponSkinsGUI.Visible = false
	end
	wait(0.5)
	InventoryOpenCooldown = false
end
MainScreenGui.Crew.BottomLeft.Skins.Activated:Connect(function()
	if InventoryOpenCooldown == false then
		InventoryOpenCooldown = true
		LoadInventory(script.Parent.MainScreenGui.WeaponSkinsGUI.Visible)
	end
end)
MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.CloseButton.Activated:Connect(function()
	LoadInventory(script.Parent.MainScreenGui.WeaponSkinsGUI.Visible)
end)
MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Code.Submit.Activated:Connect(function()
	MainEvent:FireServer("EnterPromoCode", MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Code.Entry.Input.Text)
end)
MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Unlock.Activated:Connect(function()
	PurchaseFrame("Unlock")
end)
MainScreenGui.WeaponSkinsGUI.Body.Wrapper.SkinView.Header.Unlock10x.Activated:Connect(function()
	PurchaseFrame("Unlock10x")
end)
function CrateOpening(Case,V2,Drop,ChoosenGun)
	if MainScreenGui.WeaponSkinsGUI.Visible == false then
		LoadInventory(script.Parent.MainScreenGui.WeaponSkinsGUI.Visible)
	end
	if CanOpenCrate then
		CanOpenCrate = false
		OpenCrate(Case,V2,Drop,ChoosenGun)
		CanOpenCrate = true
		return
	end
	while true do
		wait()
		if not CanOpenCrate then
		else
			break
		end
	end
	CrateOpening(Case,V2,Drop)
end
if BodyEffects.Dead.Value == true then
	if workspace.Ignored:FindFirstChild("Bell") then
		wait(4)
		workspace.Ignored:FindFirstChild("Bell"):Destroy()
	end
end

