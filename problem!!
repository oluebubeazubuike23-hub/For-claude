end);
function changefunc()
	local u1 = nil;
	u1 = {
		functions = {}, 
		connect = function(p1, p2)
			table.insert(u1.functions, p2);
		end
	};
	local u2 = nil;
	function u1.updatechanges(p3, p4)
		local v20, v21, v22 = pairs(u1.functions);
		while true do
			local v23, v24 = v20(v21, v22);
			if v23 then

			else
				break;
			end;
			v22 = v23;
			spawn(function()
				v24(p4.Press, p4.Time, u2);
			end);		
		end;
		u2 = p4.Time;
	end;
	return u1;
end;
local v25 = {};
local u3 = {};
function v25.__index(p5, p6)
	if getmetatable(u3)[p6] == nil then
		u3[p6] = {};
	end;
	return getmetatable(u3)[p6];
end;
function v25.__newindex(p7, p8, p9)
	if getmetatable(u3)[p8] == nil then
		getmetatable(u3)[p8] = {
			val = p9, 
			changed = changefunc()
		};
		return;
	end;
	getmetatable(u3)[p8].val = p9;
	getmetatable(u3)[p8].changed:updatechanges(p9);
end;
setmetatable(u3, v25);
UserInputService.InputBegan:connect(function(p10, p11)
	if not p11 then
		if p10.UserInputType ~= Enum.UserInputType.Keyboard and p10.UserInputType ~= Enum.UserInputType.Gamepad1 then
			if p10.UserInputType == Enum.UserInputType.MouseButton2 then
				u3[Enum.UserInputType.MouseButton2.Name] = {
					Press = true, 
					Time = tick()
				};
			end;
			return;
		end;
	else
		return;
	end;
	u3[p10.KeyCode.Name] = {
		Press = true, 
		Time = tick()
	};
end);


UserInputService.InputEnded:connect(function(p12, p13)
	if not p13 then
		if p12.UserInputType ~= Enum.UserInputType.Keyboard and p12.UserInputType ~= Enum.UserInputType.Gamepad1 then
			if p12.UserInputType == Enum.UserInputType.MouseButton2 then
				u3[Enum.UserInputType.MouseButton2.Name] = {
					Press = false, 
					Time = tick()
				};
			end;
			return;
		end;
	else
		return;
	end;
	u3[p12.KeyCode.Name] = {
		Press = false, 
		Time = tick()
	}
end)

