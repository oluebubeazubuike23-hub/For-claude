			v19:Play();
		end;
		if Character.Humanoid.MoveDirection.magnitude > 0 then
			v19:AdjustSpeed(1);
		else
			v19:AdjustSpeed(0);
		end;
	elseif v19.IsPlaying then
		v19:Stop();
	end;
	local v196 = MainModule.GunHold(Character);
	_G.HoldGunBool = v196;
	if v196 and BodyEffects:FindFirstChild("Block") == nil then
		local v197 = CurrentCamera.CFrame.p + CurrentCamera.CFrame.LookVector * 60;
		if _G.MobileShiftLock then
		else
			v197 = Mouse.Hit.p;
		end;
		MainRemoteEvent:FireServer("DEAHOODMOUSEPOSx3^3", v197);
