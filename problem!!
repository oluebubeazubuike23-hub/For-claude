			v917:Destroy()
		end
	end
	local v918, v919 = pcall(function()
		return game:GetService("GroupService"):GetGroupsAsync(LocalPlayer.UserId)
	end)
	if v918 and v919 then
		for _, v_u_920 in ipairs(v919) do
