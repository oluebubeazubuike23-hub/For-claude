				if v177:IsA("ImageLabel") then
					v177:Destroy()
				end
			end
			local l__Value__178 = v175[1].MaxAmmo.Value
			for v179 = 1, l__Value__178 do
				local v180 = ReplicatedStorage.AmmoG:Clone()
				v180.Name = l__Value__178 + 1 - v179
				v180.Parent = MainScreenGui.AmmoFrame.Frame
			end
		end

	end
end)
RunService:BindToRenderStep("GunAmmo", Enum.RenderPriority.Last.Value - 1, function()
	local v181 = MainModule.GunHold(Character);
	if v181 then
		if MainScreenGui.AmmoFrame.Visible == false then
			MainScreenGui.AmmoFrame.Visible = true;
		end;
		if v181[1] then
			local l__Value__182 = v181[1].Ammo.Value;
			local v183 = 0;
			if LocalPlayer.DataFolder.Inventory:FindFirstChild(v181[1].Name) then
				v183 = LocalPlayer.DataFolder.Inventory[v181[1].Name].Value;
			end;
			MainScreenGui.AmmoFrame.AmmoText.Text = v183
			for v184 = 1, v181[1].MaxAmmo.Value do
				if v184 <= l__Value__182 then
					MainScreenGui.AmmoFrame.Frame[tostring(v184)].ImageColor3 = Color3.fromRGB(255, 255, 255);
				else
					MainScreenGui.AmmoFrame.Frame[tostring(v184)].ImageColor3 = Color3.fromRGB(121, 121, 121);
				end;
			end;
			return;
		end;
	elseif MainScreenGui.AmmoFrame.Visible == true then
		MainScreenGui.AmmoFrame.Visible = false;
	end;
end);
