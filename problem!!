local v861 = MainScreenGui.Crew.BottomLeft.Frame.Report
local v_u_862 = MainScreenGui.Report
local v_u_863 = v_u_862.ReportFrame
local v_u_864 = v_u_863.ScrollingFrame
local v_u_865 = v_u_864.Template
local v_u_866 = v_u_862.SelectReport
local v_u_867 = v_u_866.SelectedPlr
local v_u_868 = v_u_862.ReportSent
local v_u_869 = nil
local v_u_870 = v_u_863.SearchBar
local function v_u_884()
	for _, v871 in ipairs(v_u_864:GetChildren()) do
		if v871:IsA("Frame") and v871.Name ~= "Template" then
			v871:Destroy()
		end
	end
	local v872 = game.Players:GetPlayers()
	table.sort(v872, function(p873, p874)
		return p873.Name:lower() < p874.Name:lower()
	end)
	local v875 = v_u_870.Text:lower()
	for _, v_u_876 in ipairs(v872) do
		if v_u_876 ~= LocalPlayer then
			local v877 = v_u_876.DisplayName:lower()
			local v878 = v_u_876.Name:lower()
			if v877:find(v875) or v878:find(v875) then
				local v879 = v_u_865:Clone()
				v879.Name = v_u_876.Name
				local v880 = v879.UserID
				local v881 = v_u_876.UserId
				v880.Text = tostring(v881)
				v879.PlayerName.Text = v_u_876.DisplayName .. " (@" .. v_u_876.Name .. ")"
				v879.PlayerAvatar.Image = "rbxthumb://type=AvatarHeadShot&id=" .. v_u_876.UserId .. "&w=150&h=150"
				v879.Visible = true
				v879.Parent = v_u_864
				v879.Report.MouseButton1Click:Connect(function()
					v_u_867.Visible = true
					local v882 = v_u_867.UserID
					local v883 = v_u_876.UserId
					v882.Text = tostring(v883)
					v_u_867.PlayerName.Text = v_u_876.DisplayName .. " (@" .. v_u_876.Name .. ")"
					v_u_867.AvatarIcon.Image = "rbxthumb://type=AvatarHeadShot&id=" .. v_u_876.UserId .. "&w=150&h=150"
					v_u_869 = v_u_876
					v_u_863.Visible = false
					v_u_866.Visible = true
				end)
			end
		end
	end
end
v_u_870:GetPropertyChangedSignal("Text"):Connect(function()
	v_u_884()
end)
local function v_u_890(p885)
	local v886 = v_u_869
	if v886 then
		MainRemoteEvent:FireServer("REPORT_PLAYER", v886, p885)
		v_u_869 = nil
		v_u_867.Visible = false
		v_u_866.Visible = false
		v_u_863.Visible = false
		v_u_868.Visible = true
		task.wait(2)
		local v887 = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local v888 = {
			["Position"] = UDim2.new(3.394, 0, -0.322, 0)
		}
		local v889 = game:GetService("TweenService"):Create(v_u_868, v887, v888)
		v889:Play()
		v889.Completed:Connect(function()
			v_u_868.Visible = false
			v_u_868.Position = UDim2.new(0.224, 0, -0.375, 0)
			v_u_863.Visible = true
			v_u_884()
		end)
	end
end
v861.MouseButton1Click:Connect(function()
	v_u_862.Visible = not v_u_862.Visible
	if v_u_862.Visible then
		v_u_863.Visible = true
		v_u_884()
	else
		v_u_863.Visible = false
		v_u_866.Visible = false
		v_u_868.Visible = false
		for _, v891 in ipairs(v_u_864:GetChildren()) do
			if v891:IsA("Frame") and v891.Name ~= "Template" then
				v891:Destroy()
			end
		end
	end
end)
