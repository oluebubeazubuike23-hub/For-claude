local v_u_830 = false
MainScreenGui.Crew.BottomLeft.Frame.Task.MouseButton1Click:connect(function()
	MainScreenGui.ChallengeTask.Visible = not MainScreenGui.ChallengeTask.Visible
	if not v_u_830 and MainScreenGui.ChallengeTask.Visible then
		v_u_830 = true
		for _, v831 in pairs(MainScreenGui.ChallengeTask.Body.TaskFrame:GetChildren()) do
			if v831:IsA("Frame") and v831.Name ~= "Template" then
				v831:Destroy()
			end
		end
		local v832, v833 = ReplicatedStorage.RemoteFunction.RemoteFunction:InvokeServer("LoadTask")
		local v834 = v833 * 604800
		local v835 = {}
		local v836 = v834 / 86400 % 31
		v835.day = math.round(v836)
		local v837 = v834 / 3600 % 24
		v835.hour = math.round(v837)
		local v838 = v834 / 60 % 60
		v835.min = math.round(v838)
		local v839 = MainScreenGui.ChallengeTask.Body.Reset
		local v840 = v835.day
		local v841 = v835.hour
		local v842 = v835.min
		v839.Text = v840 .. ":" .. v841 .. ":" .. v842
		local v843 = MainRemoteEvent.DataFolder.TaskInfo.Value
		local v844 = MainRemoteEvent.DataFolder.TaskClaimed.Value
		local v845 = game:GetService("HttpService"):JSONDecode(v843)
		local v846 = game:GetService("HttpService"):JSONDecode(v844)
		for v_u_847, v_u_848 in pairs(v832) do
			local v_u_849 = v845[v_u_848[2][1]] or 0
			local v_u_850 = MainScreenGui.ChallengeTask.Body.TaskFrame.Template:Clone()
			v_u_850.Visible = true
			v_u_850.Name = v_u_848[2][1]
			v_u_850.ImageLabel.Image = v_u_848[3]
			v_u_850.Frame.TaskText.Text = v_u_848[4]
			local v851 = v_u_850.Frame.AmountText
			local v852 = v_u_848[2][2]
			v851.Text = v_u_849 .. "/" .. v852
			local v853 = v_u_850.Frame.Frame.GreenBar
			local v854 = UDim2.new
			local v855 = v_u_849 / v_u_848[2][2]
			v853.Size = v854(math.clamp(v855, 0, 1), 0, 1, 0)
			v_u_850.Frame.RewardText.Text = v_u_848[1][2]
			if v846[tostring(v_u_847)] then
				v_u_850.Frame.ClaimButton.Text = "CLAIMED"
			else
				v_u_850.Frame.UIStroke.Enabled = false
			end
			v_u_850.Parent = MainScreenGui.ChallengeTask.Body.TaskFrame
			v_u_850.Frame.ClaimButton.MouseButton1Click:Connect(function()
				MainRemoteEvent:FireServer("ClaimTask", v_u_847)
				if v_u_849 >= v_u_848[2][2] then
					v_u_850.Frame.UIStroke.Enabled = true
					v_u_850.Frame.ClaimButton.Text = "CLAIMED"
				end
			end)
		end
		task.wait(1)
		v_u_830 = false
	end
end)
MainScreenGui.ChallengeTask.Body.CloseButton.MouseButton1Click:connect(function()
	MainScreenGui.ChallengeTask.Visible = false
end)

