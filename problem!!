for v308, v309 in pairs(workspace.Ignored.Shop:GetChildren()) do
	if v309:FindFirstChild("Highlight") then
		v309.Highlight:Destroy()
	end
	if v309:FindFirstChild("ClickDetector") then
		local lClickDetector310 = v309.ClickDetector
		lClickDetector310.MouseHoverEnter:Connect(function()
			local v311 = Instance.new("Highlight", v309)
			v311.DepthMode = Enum.HighlightDepthMode.Occluded
			v311.FillTransparency = 1
			v311.OutlineTransparency = 1
			v311.OutlineColor = Color3.fromRGB(0)
		end)
		lClickDetector310.MouseHoverLeave:Connect(function()
			if v309:FindFirstChild("Highlight") then
				v309.Highlight:Destroy()
			end
		end)
	end
end

local u25 = {};
local u26 = { "19323823", "19323854" };
local u27 = { "19003948", "19003957", "19003971", "19003978", "19003982", "19003990", "19003998" };


MainRemoteEvent.OnClientEvent:connect(function(p59, ...)
	if p59 == "FX_KILL" then
		local StompFXName, VictimHRP, AttackerPlayer = ...

		print("[CLIENT FX] Received FX_KILL:", StompFXName, VictimHRP, AttackerPlayer.Name)

		-- Find the stomp FX module
		local StompFXS = ReplicatedStorage:FindFirstChild("StompFXS")
		if not StompFXS then
			warn("[CLIENT FX] StompFXS folder not found!")
			return
		end

		local FXModule = StompFXS:FindFirstChild(StompFXName)
		if not FXModule then
			warn("[CLIENT FX] Stomp module not found:", StompFXName)
			FXModule = StompFXS:FindFirstChild("Regular")
			if not FXModule then
				return
			end
		end

		-- Run the FX
		pcall(function()
			local StompScript = require(FXModule)
			if StompScript.RunStompFx then
				local VictimChar = VictimHRP.Parent
				StompScript.RunStompFx(StompFXName, VictimHRP, VictimChar, LocalPlayer, {})
			end
		end)
		return
	end
