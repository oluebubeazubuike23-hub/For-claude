	v_u_862.Visible = false
	v_u_863.Visible = false
	v_u_866.Visible = false
	v_u_868.Visible = false
	for _, v892 in ipairs(v_u_864:GetChildren()) do
		if v892:IsA("Frame") and v892.Name ~= "Template" then
			v892:Destroy()
		end
	end
end)
