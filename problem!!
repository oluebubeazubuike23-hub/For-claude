BodyEffects["K.O"].Changed:Connect(function(Ragdolled)
	if Ragdolled then
		CurrentCamera.CameraSubject = Character.UpperTorso 
		StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false) 
		return 
	end 
	CurrentCamera.CameraSubject = Character.Humanoid 
	StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true) 
end) 

local Money = LocalPlayer.DataFolder.Currency.Value
local function makeMoneyChangeGUI(Currency)
	script.CurrencySound:Play() 
	local Color
	if Currency > 0 then
		Color = Color3.fromRGB(95, 255, 87) 
		Currency = "+ $" .. MainModule.AddComma(Currency) 
	else
		Color = Color3.fromRGB(194, 0, 0) 
		Currency = "- $" .. MainModule.AddComma(math.abs(Currency)) 
	end 
	local TextLabel = Instance.new("TextLabel") 
	TextLabel.Name = "MONEYDELTA" 
	TextLabel.Text = Currency 
	TextLabel.BackgroundTransparency = 1 
	TextLabel.AnchorPoint = Vector2.new(0.5, 0.5) 
	TextLabel.Position = UDim2.new(0.5, 0, 0, 40) 
	TextLabel.TextColor3 = Color 
	TextLabel.Size = UDim2.new(0, 50, 0, 50) 
	TextLabel.Font = Enum.Font.GothamBold
	TextLabel.TextSize = 25 
	TextLabel.Parent = MainScreenGui 
	delay(0.3, function()
		game:GetService("TweenService"):Create(TextLabel, TweenInfo.new(1), {
			Position = UDim2.new(0.5, 0, 0, -40), 
			TextTransparency = 1
		}):Play() 
		wait(1) 
		TextLabel:Destroy() 
	end) 
end 

LocalPlayer:WaitForChild("DataFolder").Currency.Changed:connect(function()
	MainScreenGui.MoneyText.Text = "$" .. MainModule.AddComma(LocalPlayer:WaitForChild("DataFolder").Currency.Value) 
	if Money ~= nil then
		makeMoneyChangeGUI(LocalPlayer:WaitForChild("DataFolder").Currency.Value - Money) 
	end 
	Money = LocalPlayer:WaitForChild("DataFolder").Currency.Value 
end)
MainScreenGui.MoneyText.Text = "$" .. MainModule.AddComma(LocalPlayer:WaitForChild("DataFolder").Currency.Value) 
BodyEffects:WaitForChild("GunShotChanges")
BodyEffects.GunShotChanges.Changed:connect(function()
	local v36 = MainModule.GunHold(Character);
	if v36 == nil or v36 == false then
		return;
	end;
	local v__boolean__37 = true;
	if v36 and v36[1] and v36[1].Ammo.Value <= 0 then
		v__boolean__37 = false;
	end;
	if v36[1].Name == "[Flamethrower]" then
		return;
	end;
	if not (not _G.Aimed) or v36[1].Name == "[Shotgun]" or v36[1].Name == "[Rifle]" or v36[1].Name == "[TacticalShotgun]" or v36[1].Name == "[AR]" or v36[1].Name == "[AUG]" or v36[1].Name == "[AK47]" or v36[1].Name == "[Scar]" or v36[1].Name == "[LMG]" or v36[1].Name == "[SilencerAR]" then
		if v__boolean__37 then
			if v36[1].Name ~= "[Minigun]" then
				v16:Play();
			end
			return;
		end;
	else
		if v__boolean__37 then
			if v36[1].Name ~= "[Minigun]" then
				v17:Play(0.100000001, 1, 1.25);
			end
		end;
	end;
end)
function ReplicatedStorage.RemoteFunction.RemoteFunction.OnClientInvoke(p57, ...)
	if p57 == "Aim" then
		local v39, v40 = ...;
		local v41 = MainModule.GunHold(Character);
		if v41 == nil or v41 == false then
			return;
		else
			local v__boolean__42 = true;
			if v41 and v41[1] and v41[1].Ammo.Value <= 0 then
				v__boolean__42 = false;
			end;
			local v43
			if not (not _G.Aimed) or v41[1].Name == "[Shotgun]" or v41[1].Name == "[Rifle]" or v41[1].Name == "[AR]" then
				v43 = (Mouse.Hit.p - v39).unit;
				if v__boolean__42 then

					v16:Play();
				end;
			else
				v43 = (Mouse.Hit.p + Vector3.new(0, math.random() > 0.5 and math.random() or math.random() * -1, 0) - v39).unit;
				if v__boolean__42 then
					v17:Play(0.100000001, 1, 1.25);
				end;
			end;
			local v44
			if _G.GUN_COMBAT_TOGGLE then
				v44 = nil;
				if v40 ~= true then
					for v45, v46 in pairs(workspace.Players:GetChildren()) do
						if v46 ~= Character and v46:FindFirstChild("Humanoid") and v46.Humanoid.Health > 0 and v46:FindFirstChild("UpperTorso") and (v46.UpperTorso.Position - Character.UpperTorso.Position).magnitude < 5 then
							return true;
						end;
					end;
					v44 = v43;
					return v44;
				else
					v44 = v43;
					return v44;
				end;
			else
				v44 = v43;
				return v44;
			end;
		end;
	end;
	if p57 == "MOUSEPOS" then
		return Mouse.Hit.p;
	end;
	if p57 == "CLOSECOMBAT" then
		for v47, v48 in pairs(workspace.Players:GetChildren()) do
			if v48 ~= Character and v48:FindFirstChild("Humanoid") and v48.Humanoid.Health > 0 and v48:FindFirstChild("UpperTorso") and (v48.UpperTorso.Position - Character.UpperTorso.Position).magnitude < 5 then
				return true;
			end;
		end;
		return false;
	end;
	if p57 ~= "LOCK_PICK" then
		if p57 == "Join/Leave" then
			local l__AreYouSure__49 = MainScreenGui.AreYouSure;
			l__AreYouSure__49.Visible = true;
			local u19 = nil;
			local v50 = l__AreYouSure__49.TextButton2.Activated:connect(function()
				u19 = false;
			end);
			while true do
				task.wait();
				if u19 ~= nil then
					break;
				end;			
			end;
			l__AreYouSure__49.TextButton.Activated:connect(function()
				u19 = true;
			end):disconnect();
			v50:disconnect();
			l__AreYouSure__49.Visible = false;
			return nil;
		else
			return;
		end;
	end;
	local l__LockPick__51 = MainScreenGui.LockPick;
	l__LockPick__51.Visible = true;
	local u20 = false;
	local u21 = nil;
	u21 = Mouse.Button1Down:connect(function()
		u20 = true;
		u21:disconnect();
	end);
	local v52 = 0;
	while u20 == false do
		v52 = v52 + 0.01;
		local v53 = v52 % 1;
		if v53 > 0.3 and v53 < 0.7 then
			v52 = v52 + 0.01;
		end;
		l__LockPick__51.Green.Position = UDim2.new(v52 % 1, 0, 0.5, 0);
		game:GetService("RunService").Heartbeat:wait();	
	end;
	local v54 = v52 % 1;
	l__LockPick__51.Visible = false;
	if v54 > 0.4 and v54 < 0.6 then
		return true;
	end;
	return false;
end;
CurrentCamera:ClearAllChildren();
